                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  name="OLE SRC Extract"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/****** Script for SelectTopNRows command from SSMS  ******/
SELECT 
ISNULL((SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=[Sales Invoice Line].[DWNavisionCompany] AND [Department Code]=sih.[Department Code])
	   ,(SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=[Sales Invoice Line].[DWNavisionCompany] AND [Department Code]='')) AS [CompanyKey]

,CONVERT(NVARCHAR(20),[Sales Invoice Line].[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30),[Sales Invoice Line].[No.]) AS [ItemKey]
,CONVERT(NVARCHAR(20),[Sales Invoice Line].[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), [Sales Invoice Line].[Sell-to Customer No.]) AS [CustomerSellToKey]
,CASE 
	WHEN LEN(cust.[Bill-to Customer No.]) &gt; 0 
		THEN CONVERT(NVARCHAR(20),[Sales Invoice Line].[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), cust.[Bill-to Customer No.])
		ELSE CONVERT(NVARCHAR(20),[Sales Invoice Line].[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sih.[Bill-to Customer No.]) 
 END AS [CustomerBillToKey]
,[Sales Invoice Line].[Posting Date] AS CalendarKey
,[Sales Invoice Line].[Posting Date] AS CalendarOrderKey -- order date
,CONVERT(NVARCHAR(20),[Sales Invoice Line].[Document No.]) AS InvoiceNo
,CONVERT(NVARCHAR(10),sih.[Currency Code]) AS CurrencyKey
,CONVERT(NVARCHAR(20),[Sales Invoice Line].[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),[Sales Invoice Line].[Unit Price Last modified by]) AS EmployeeLastChangeKey
-- GPPG?
,CONVERT(NVARCHAR(10), it.[Gen. Prod. Posting Group]) AS GPPGCurrentKey
,CONVERT(NVARCHAR(10), [Sales Invoice Line].[Gen. Prod. Posting Group]) AS GPPGHistoricalKey
      ,[Amount] AS [InvoiceAmount]
      ,[Quantity] AS [InvoicedQuantity]

-- ,[Posting Date]
--,[Document No.]
--      ,[Line No.]
--      ,[Sales Invoice Line].[Sell-to Customer No.]
--      ,[Type]
--      ,[Sales Invoice Line].[No.]
--      ,[Unit Price]

--      ,[Amount (LCY)]
--	  ,[Amount (LCY)]/[Amount] AS RateCurr
--	  ,([Quantity]*[Unit Price]-[Quantity]*[Original Unit Price])*[Amount (LCY)]/[Amount] AS DIFFLCY
--      ,[Requested Price]
--      ,[Original Unit Price]
--      ,[Unit Price Last modified by]

--      ,[Sales Invoice Line].[DWNavisionCompany]
--      ,[Sales Invoice Line].[DWCreatedDate]
   --   SUM([Amount (LCY)]) AS Amount
	  --,SUM(([Quantity]*[Unit Price]-[Quantity]*[Original Unit Price])*([Amount (LCY)]/[Amount])) AS Original2ActualAmount
	  --,SUM(CASE WHEN [Requested Price]&gt;0 THEN ([Quantity]*[Requested Price]-[Quantity]*[Original Unit Price])*([Amount (LCY)]/[Amount]) ELSE 0.0 END) AS Original2RequestAmount
	  	  ,([Quantity]*[Unit Price]-[Quantity]*[Original Unit Price])*([Amount (LCY)]/[Amount]) AS Original2ActualAmount
	 ,CASE WHEN [Requested Price]&gt;0 THEN ([Quantity]*[Requested Price]-[Quantity]*[Original Unit Price])*([Amount (LCY)]/[Amount]) ELSE 0.0 END AS Original2RequestAmount

  FROM [EETDW_Extract].[nav].[Sales Invoice Line]

  INNER JOIN [EETDW_Extract].[nav].[Sales Invoice Header] sih ON sih.[No.]=[Sales Invoice Line].[Document No.]  AND sih.DWNavisionCompany = [Sales Invoice Line].DWNavisionCompany  --AND sih.[Order No.] = [Sales Invoice Line].[Order No.] 
  
     LEFT JOIN nav.Customer AS cust
   ON cust.[No.] = sih.[Bill-to Customer No.] AND cust.DWNavisionCompany = [Sales Invoice Line].DWNavisionCompany

   LEFT JOIN nav.Item AS it
   ON it.[No.] = [Sales Invoice Line].[No.] AND it.DWNavisionCompany = [Sales Invoice Line].DWNavisionCompany

  WHERE [Original Unit Price]&gt;0 
  AND [Unit Price]&gt;0
    --AND  [Amount]&lt;&gt;[Amount (LCY)] 
  AND [Amount]&lt;&gt;0
  -- remove outliers
  AND  ([Unit Price]/[Original Unit Price]) BETWEEN 0.1 AND 10

--AND [Unit Price]&gt;[Original Unit Price]

--ORDER BY 1

  --2.026.220

-- -303.049.226</property>
