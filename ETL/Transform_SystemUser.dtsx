                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
       CONVERT(NVARCHAR(30), [UserId]) AS SystemUserKey
      ,CONVERT(NVARCHAR(30), [UserId]) AS SystemUserName
      ,CONVERT(NVARCHAR(10),
	   CASE WHEN [Super user] = 1 THEN 'Yes' ELSE 'No' END) 
	   AS IsSuperUserIndicator
  FROM [EETDW_Extract].[navcom].[BI User Access]

union all

SELECT DISTINCT
 CONVERT(NVARCHAR(30),
--TEST
case when [UserId] = 'JGO-DK' then 'EETNORDIC\nna-ext' else 'a' END) AS SystemUserKey
      ,CONVERT(NVARCHAR(30), [UserId]) AS SystemUserName
      ,CONVERT(NVARCHAR(10),
	   CASE WHEN [Super user] = 1 THEN 'Yes' ELSE 'No' END) 
	   AS IsSuperUserIndicator
  FROM [EETDW_Extract].[navcom].[BI User Access]
  where [UserId] = 'JGO-DK'</property>
