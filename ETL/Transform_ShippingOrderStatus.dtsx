                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
	 CONVERT(INT,[Status]) AS ShippingOrderStatusKey
	,CONVERT(nvarchar(20),
		CASE [Status]
			WHEN '0' THEN N'In Stock'
			WHEN '1' THEN N'Confirmed'
			WHEN '2' THEN N'Delayed'
			WHEN '3' THEN N'Estimated'
			WHEN '4' THEN N'Default'
			WHEN '5' THEN N'Unknown'
		END
	) AS ShippingOrderStatusName
FROM
	[EETDW_Extract].[nav].[Shipment/Receipt Date Log]
WHERE
	Active = '1'
	AND [Type] = 2</property>
