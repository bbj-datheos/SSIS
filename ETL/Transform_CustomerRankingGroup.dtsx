                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/****** Script for SelectTopNRows command from SSMS  ******/
/*
  SELECT 
      max([rank0]) AS RankID
	  , 0 AS RankTypeID
      ,[CustomerRankingGroupIntermediateKey]
	  ,max([CustomerBillToKey])
  FROM [EETDW_Stage].[dbo].[CustomerRankingGroupIntermediate]
  WHERE [rank0] IS NOT NULL AND [rank0] &lt;= 200
  group by [CustomerRankingGroupIntermediateKey]
  order by RankID
*/

  SELECT 
      [rank0] AS RankID
	  , 0 AS RankTypeID
      ,[CustomerRankingGroupIntermediateKey]
  FROM [EETDW_Stage].[dbo].[CustomerRankingGroupIntermediate]
  WHERE [rank0] IS NOT NULL AND [rank0] &lt;= 200


  UNION

    SELECT 
      [rank1] AS RankID
	  , 1 AS RankTypeID
      ,[CustomerRankingGroupIntermediateKey]
  FROM [EETDW_Stage].[dbo].[CustomerRankingGroupIntermediate]
  WHERE [rank1] IS NOT NULL AND [rank1] &lt;= 100

  UNION

    SELECT 
      [rank2] AS RankID
	  , 2 AS RankTypeID
      ,[CustomerRankingGroupIntermediateKey]
  FROM [EETDW_Stage].[dbo].[CustomerRankingGroupIntermediate]
  WHERE [rank2] IS NOT NULL AND [rank2] &lt;= 100

	UNION

	SELECT 
		[rank3] AS RankID
		, 3 AS RankTypeID
		,[CustomerRankingGroupIntermediateKey]
	FROM [EETDW_Stage].[dbo].[CustomerRankingGroupIntermediate]
	WHERE [rank3] IS NOT NULL AND [rank3] &lt;= 100

	UNION

