                <component
                  refId="Package\SEQC Transform Purchase\DFT Transform Purchase\OLE SRC Extract"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  name="OLE SRC Extract"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT --TOP 1000
--pl.[Shipped Not Invoiced],
--pl.[Qty. Rcd. Not Invoiced],
--pl.[Outstanding Quantity],
ISNULL((SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=pl.[DWNavisionCompany] AND [Department Code]=pl.[Department Code])
	   ,(SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=pl.[DWNavisionCompany] AND [Department Code]='')) AS [CompanyKey]
,CONVERT(NVARCHAR(20),pl.[Document No.]) AS PurchaseNo
,CONVERT(NVARCHAR(30), pl.[Line No.]) AS PurchaselineNo
,CONVERT(NVARCHAR(20),pl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), pl.[Buy-from Vendor No.]) AS [VendorBuyFromKey]
--,CASE 
--	WHEN LEN(cust.[Bill-to Vendor No.]) &gt; 0 
--		THEN CONVERT(NVARCHAR(20),pl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), cust.[Bill-to Vendor No.])
--		ELSE CONVERT(NVARCHAR(20),pl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), pl.[Buy-from Vendor No.]) 
-- END AS [VendorBillToKey]
,CONVERT(NVARCHAR(20),pl.[DWNavisionCompany]) + N'-' + CASE WHEN pl.[Type]=3 THEN N'RES_' ELSE N'' END + CONVERT(NVARCHAR(50), pl.[No.]) AS ItemKey
,pl.[Expected Receipt Date] AS CalendarKey
,pl.[Order Date] AS CalendarOrderKey
--,CONVERT(NVARCHAR(20),sh.[Origin of order]) AS OriginOfOrderKey

-- LOOK INTO THIS
,CASE WHEN (pl.[Currency Code] = '' OR pl.[Currency Code] IS NULL)
	THEN 
	  CASE WHEN (sh.[Currency Code] = '' OR sh.[Currency Code] IS NULL)
	  THEN ISNULL((SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=sh.[DWNavisionCompany] AND [Department Code]=sh.[Department Code])
                 ,(SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=sh.[DWNavisionCompany] AND [Department Code]=''))
	  ELSE sh.[Currency Code]
	END  
	  ELSE pl.[Currency Code]
 END AS [CurrencyKey]

--,CASE 
--	WHEN LEN(pl.[RMA No]) &gt; 0 
--		THEN N'1-0-0' 
--		ELSE N'0-0-0' 
-- END AS [OrderTypeKey]
--,CONVERT(NVARCHAR(20),pl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),pl.[Cost changed Reasoncode]) AS CostChangeReasonKey
--,CONVERT(NVARCHAR(20),pl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sh.[Purchaseperson Code]) AS EmployeeKey
--,CONVERT(INT,pn.ID) AS PersonKey
--,CONVERT(NVARCHAR(20),sh.[Order Status]) + N'-' + CONVERT(NVARCHAR(20),sh.[Hold invoice]) AS OrderStatusKey
,CONVERT(NVARCHAR(20),pl.[Receipt Date Status]) AS PurchaseOrderStatusKey
,CONVERT(nvarchar(1),pl.[Receipt Date Status]) +N'-' + CONVERT(nvarchar(10),CONVERT(date,pl.[Expected Receipt Date])) AS ReceiptDateStatusKey ---SRG: 20200310

--,CASE 
--      WHEN pl.[Qty. Rcd. Not Invoiced]&lt;&gt;0 AND pl.Quantity&lt;&gt;0 THEN (pl.[Amount]*pl.[Qty. Rcd. Not Invoiced]/pl.Quantity)
--	  WHEN pl.[Qty. Rcd. Not Invoiced]=0 AND pl.[Outstanding Quantity]&lt;&gt;0 AND pl.Quantity&lt;&gt;0 THEN (pl.[Amount]*pl.[Outstanding Quantity]/pl.Quantity)
--         ELSE 0 END AS PurchaseAmount -- 20180307 added by bbj after agreement with Jan Iversen
--,CASE 
--      WHEN pl.[Qty. Rcd. Not Invoiced]&lt;&gt;0 AND pl.Quantity&lt;&gt;0 THEN pl.[Order Cost Amount (LCY)]*-1*pl.[Qty. Rcd. Not Invoiced]/pl.Quantity
--	  WHEN pl.[Qty. Rcd. Not Invoiced]=0 AND pl.[Outstanding Quantity]&lt;&gt;0 AND pl.Quantity&lt;&gt;0 THEN pl.[Order Cost Amount (LCY)]*-1*pl.[Outstanding Quantity]/pl.Quantity
--         ELSE 0 END AS OrderCostAmount -- Purchase Cost
--,CASE 
--      WHEN pl.[Qty. Rcd. Not Invoiced]&lt;&gt;0 AND pl.Quantity&lt;&gt;0 THEN (pl.[Order Profit Margin (LCY)]*pl.[Qty. Rcd. Not Invoiced]/pl.Quantity)
--	  WHEN pl.[Qty. Rcd. Not Invoiced]=0 AND pl.[Outstanding Quantity]&lt;&gt;0 AND pl.Quantity&lt;&gt;0 THEN (pl.[Order Profit Margin (LCY)]*pl.[Outstanding Quantity]/pl.Quantity)
--         ELSE 0 END AS OrderProfitMarginAmount -- Purchase Profit
--,CASE 
--      WHEN pl.[Qty. Rcd. Not Invoiced]&lt;&gt;0 AND pl.Quantity&lt;&gt;0 THEN pl.[Unit Cost (LCY)]*-1*pl.[Qty. Shipped Not Invoiced]/pl.Quantity
--	  WHEN pl.[Qty. Rcd. Not Invoiced]=0 AND pl.[Outstanding Quantity]&lt;&gt;0 AND pl.Quantity&lt;&gt;0 THEN pl.[Unit Cost (LCY)]*-1*pl.[Outstanding Quantity]/pl.Quantity
--        ELSE pl.[Unit Cost (LCY)]*-1 END AS UnitCostAmount -- Financial cost


,CASE WHEN pl.[VAT %]=0 THEN 
     CASE WHEN ISNULL(sh.[Currency Factor],0)=0 THEN pl.[Amount] ELSE pl.[Amount]/sh.[Currency Factor] END
   ELSE CASE WHEN ISNULL(sh.[Currency Factor],0)=0 THEN pl.[Amount] ELSE pl.[Amount]/sh.[Currency Factor] END -- NB pl.[Amount] already ex. VAT /(1+pl.[VAT %]/100) 
 END AS [OrderBeforeDeliveryAmount]--[Amount (LCY)] NB LCY Amounts are incl. VAT
,pl.Quantity AS [OrderBeforeDeliveryQuantity]
,CASE WHEN pl.[VAT %]=0 THEN pl.[Outstanding Amount (LCY)] ELSE pl.[Outstanding Amount (LCY)]/(1+pl.[VAT %]/100) END AS [OutstandingAmount]
,pl.[Outstanding Quantity] AS [OutstandingQuantity]
,CASE WHEN pl.[VAT %]=0 THEN pl.[Outstanding Amount (LCY)] + pl.[Amt. Rcd. Not Invoiced (LCY)] ELSE (pl.[Outstanding Amount (LCY)] + pl.[Amt. Rcd. Not Invoiced (LCY)])/(1+pl.[VAT %]/100) END AS [PurchaseInvoiceOpenAmount] --+ [Amt. Rcd. Not Invoiced]
,pl.[Outstanding Quantity] + pl.[Qty. to Invoice] AS [PurchaseInvoiceOpenQuantity]

,CONVERT(NVARCHAR(100),N'PO-' + pl.[Document No.]) AS DocumentKey
--,getdate() AS [DWCreatedDate]
--INTO [EETDW_Stage].[dbo].[PurchaseOrder]
-- added by bbj 20180913
,pl.[DWNavisionCompany]+N'-'+pl.[Owner] +N'-'+ pl.[Location Code] AS [InventoryTypeKey]
--end bbj

FROM [EETDW_Extract].[nav].[Purchase Line] AS pl
 LEFT JOIN nav.Vendor AS cust
   ON cust.[No.] = pl.[Buy-from Vendor No.] AND cust.DWNavisionCompany = pl.DWNavisionCompany
 LEFT JOIN nav.[Purchase Header] AS sh
   ON sh.[No.] = pl.[Document No.] AND sh.DWNavisionCompany = pl.DWNavisionCompany 
-- LEFT JOIN nav.[Cost changed Reasoncodes] ccr 
--   ON ccr.DWNavisionCompany = pl.DWNavisionCompany AND ccr.Code = pl.[Cost changed Reasoncode]
--LEFT JOIN navcom.[System User] su
--  ON su.[Primary Company] = sh.DWNavisionCompany AND su.[User ID] = sh.[Purchaseperson Code]
--LEFT JOIN EETDW_Stage.map.PersonNavision pn on pn.SystemUser = sh.[Purchaseperson Code] 
WHERE pl.[Document Type] = 1
and pl.[Type] &lt;&gt; ''
and pl.[No.]&lt;&gt;''
-- added by bbj 20180327
--and pl.Quantity&lt;&gt;0 not needed since included in cas on values
--and pl.Quantity=0
-- end bbj
and (pl.[Outstanding Quantity]&lt;&gt;0 OR pl.[Qty. Rcd. Not Invoiced]&lt;&gt;0)
--and [Document No.]='148114'
--ORDER BY pl.[Expected Receipt Date]

--((pl.[Outstanding Quantity]&lt;&gt;0 AND sh.[Hold invoice]=0) OR (sh.[Hold invoice]=1 AND pl.[Outstanding Quantity]=0)) -- 20180307 added by bbj after agreement with Jan Iversen
--and sh.[Hold invoice]=1
--and pl.DWNavisionCompany='EET Finland'
--and pl.[Qty. Shipped Not Invoiced]&lt;&gt;0</property>
