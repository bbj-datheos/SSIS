                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	 CONVERT(NVARCHAR(50),[DWNavisionCompany] + N'-' + [Code])		AS [EmployeeKey]
	,CONVERT(NVARCHAR(10), [Code])									AS [EmployeeCode]
	,CONVERT(NVARCHAR(50), [Name])									AS [EmployeeName]
	,CONVERT(NVARCHAR(30), [User-Id])								AS [EmployeeUserCode]
	,CONVERT(NVARCHAR(10), [Sales Group])							AS [EmployeeSalesGroupCode]
	,CONVERT(NVARCHAR(20),[DWNavisionCompany])						AS [CompanyCode]
FROM
	[nav].[Salesperson/Purchaser]

UNION

SELECT DISTINCT
	 CONVERT(nvarchar(30),C.[DWNavisionCompany] + N'-' +[User ID])	AS EmployeeKey
	,CONVERT(nvarchar(10),[User ID])								AS EmployeeCode
	,CONVERT(nvarchar(50),N'')										AS [EmployeeName]
	,CONVERT(nvarchar(30),[User ID])								AS [EmployeeUserCode]
	,CONVERT(nvarchar(10),N'')										AS [EmployeeSalesGroupCode]
	,CONVERT(nvarchar(20),C.[DWNavisionCompany])					AS [CompanyCode]
FROM
	[EETDW_Extract].[nav].[Cust_ Ledger Entry] C
	LEFT JOIN (
		SELECT DISTINCT
			 [Code]
			,[DWNavisionCompany]
		FROM
			[nav].[Salesperson/Purchaser]
		WHERE [User-Id] &lt;&gt; ''
	) SP
	ON C.[User ID] = SP.[Code]
	AND C.DWNavisionCompany = SP.DWNavisionCompany
WHERE
	[User ID] &lt;&gt; ''
	AND (SP.DWNavisionCompany is null and SP.[Code]  is null)</property>
