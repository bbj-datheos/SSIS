                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	 CallID as ActivityNo
	,ps.ID AS PersonKey
	,CAST(CONVERT(DATE,[DateTime]) AS DATETIME) AS CalendarKey
	,DATEPART(HOUR,[DateTime]) as TimeHourKey
	,DATEPART(MINUTE,[DateTime]) as TimeMinuteKey
	,DATEPART(SECOND,[DateTime]) as TimeSecondKey
	,CONVERT(nvarchar(40),'C-' + left(Direction,1) + '-' + left([A Type],1) + '-' + 
		 left([B Type],1) + '-' + left(GroupCall,1) + '-' + 
		 iif(CallType like '%err%', 'Y', 'N') + '-' +
		 'N')
		 as CommunicationTypeKey
	--,a.BusinessUnit
	,CONVERT(NVARCHAR(10),bu.Code) AS CompanyKey
	,CONVERT(NVARCHAR(2),'C') AS PointTypeKey
	,CAST(ROUND(ResponseTime/60,0) AS INT) AS ResponseTimeMinutes
	,CAST(ROUND(Duration/60,0) AS INT) AS DurationMinutes
	,ResponseTimeDivided AS ResponseTimeSecondsDivided
	,DurationDivided AS DurationSecondsDivided
	,1 AS ActivityCount
	,CAST(ROUND(Duration/60,0) AS INT) AS SalesUnitPoints

FROM exch.UC_Analytics a
LEFT JOIN EETDW_Stage.map.PersonSkype ps ON a.EmployeeExtension = ps.SkypeUser
LEFT JOIN 
		(select * from navcom.[Business Unit] bu where Consolidate = 1) bu 
		ON bu.[Company Name] = a.BusinessUnit
