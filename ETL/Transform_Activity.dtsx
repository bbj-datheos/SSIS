                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
	 UniqueID as ActivityNo
	,pc.ID as PersonKey
	,cast(convert(date,StartTime) as datetime) as CalendarKey 
	,datepart(hour,StartTime) as TimeHourKey
	,datepart(minute,StartTime) as TimeMinuteKey
	,datepart(second,StartTime) as TimeSecondKey
    ,'M-' + iif(len(left(isnull(Categories, ''), 1)) = 1, left(Categories, 1) + '-', '')  +
		iif(charindex(';', RequiredAttendees) &gt; 1, 'Y', 'N') + '-' + 
		iif(IsCancelled = 'False', 'N', 'Y') + '-' +
		iif(StartTime &gt; getdate(), 'Y', 'N')
	    as CommunicationTypeKey
	,bu.Code as CompanyKey
	,CONVERT(NVARCHAR(2),'M') as PointTypeKey
	,0 as ResponseTimeMinutes
	,cast(convert(numeric(19,6),datediff(minute,StartTime,EndTime)) as int) as DurationMinutes
	,convert(decimal(19,6), 0) AS ResponseTimeSecondsDivided
	,convert(numeric(19,6),datediff(second,StartTime,EndTime)/86400) as DurationSecondsDivided
	,1 AS ActivityCount
	,cast(convert(numeric(19,6),datediff(minute,StartTime,EndTime))*10 as int) as SalesUnitPoints
  FROM [EETDW_Extract].[exch].[Calendar] c
  left join [EETDW_Extract].navcom.Company co on [Company Country Code] = c.CompanyId
  left join 
			(select
				 [Company Name]
				,Code
				,Consolidate
			 from [EETDW_Extract].navcom.[Business Unit]
			 where Consolidate = 1) bu on bu.[Company Name] = co.Name 
  inner join EETDW_Stage.map.PersonCalendar pc on c.MailBox = pc.CalendarEmail
--where convert(date, StartTime) &gt; dateadd(month, -1, convert(date, getdate()))
