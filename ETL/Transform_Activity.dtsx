	select 
      [MessageId]
      ,cast(LEFT(recipients, charindex(';',recipients+';')-1) AS NVARCHAR(4000))
	  ,stuff(recipients, 1, charindex(';',recipients+';'), '')
	from EETDW_Extract.exch.Emails 
	union all
	select 
	   [MessageId]
      ,cast(LEFT(recipients, charindex(';',recipients+';')-1) AS NVARCHAR(4000))
	  ,stuff(recipients, 1, charindex(';',recipients+';'), '')
	from stuf
	where recipients &gt; ''),
	main as (
	select
		e.MessageId
		,result as Email
		,MessageSubject
		,SenderType
		,RecipientType
		,e.recipients
		,Direction
		,Timestamp
		from EETDW_Extract.exch.Emails e
		inner join stuf s on e.MessageId = s.MessageId
		union all
	select 
		MessageId
		,sender as Email
		,MessageSubject
		,SenderType
		,[RecipientType] -- direction of message; productive if other party is external
		,recipients
		,Direction 
		,[Timestamp]
	from EETDW_Extract.exch.Emails)

	select
	 MessageId as AcitivityNo
	,pe.ID as PersonKey
	,cast(convert(date,e.[Timestamp]) as datetime) as CalendarKey 
	,datepart(hour,e.[Timestamp]) as TimeHourKey
	,datepart(minute,e.[Timestamp]) as TimeMinuteKey
	,datepart(second,e.[Timestamp]) as TimeSecondKey
	,'E' + '-' + left(e.Direction, 1) + '-' + left(SenderType, 1) + '-' +
		left(RecipientType, 1) + '-' +
		iif(charindex(';', recipients) &gt; 1, 'Y', 'N') +
		'-N-N' 
		as CommunicationTypeKey
	,bu.Code as CompanyKey
	,CONVERT(NVARCHAR(2),'E') as PointTypeKey
	,0 as ResponseTimeMinutes
	,DurationSeconds/60 as DurationMinutes
	,convert(decimal(19,6), 0) AS ResponseTimeSecondsDivided
	,convert(decimal(19,6), convert(decimal(19,6), DurationSeconds)/86400.0) as DurationSecondsDivided
	,1 as ActivityCount
	,(DurationSeconds/60)*10 as SalesUnitPoints
	from main e
	inner join EETDW_Stage.map.PersonEmail pe on e.Email = pe.Email
	left join [EETDW_Extract].navcom.Company co on [Company Country Code] = right(e.Email, 2)
	left join 
			(select
				 [Company Name]
				,Code
				,Consolidate
			 from [EETDW_Extract].navcom.[Business Unit]
			 where Consolidate = 1) bu on bu.[Company Name] = co.Name
	left join EETDW_Extract.control.EmailDuration ed on ed.PointTypeName = 'Email' and ed.CommunicationTypeDirection = e.Direction 
--where convert(date, e.Timestamp) &gt; dateadd(month, -1, convert(date, getdate()))
option (maxrecursion 0);</property>
