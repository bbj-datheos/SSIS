                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT --TOP 1000 
	[Entry No.]
	,N'GLE-' + CAST ([Document Type] AS NVARCHAR) + N'-' +  [Document No.] AS DocumentKey
	,CASE [Document Type]
		WHEN 0 THEN N''
		WHEN 1 THEN N'Payment'
		WHEN 2 THEN N'Invoice'
		WHEN 3 THEN N'Credit Memo'
		WHEN 4 THEN N'Finance Charge Memo'
		WHEN 5 THEN N'Reminder'
		ELSE N'Unknown'
	END AS [Document Type]
	,[Document No.] AS [Document No]
	,[Posting Date] AS [CalendarKey]
	,ISNULL((SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=gle.[DWNavisionCompany] AND [Department Code]=gle.[Department Code])
		,(SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=gle.[DWNavisionCompany] AND [Department Code]='')) AS [CompanyKey]
	,ISNULL((SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=gle.[DWNavisionCompany] AND [Department Code]=gle.[Department Code])
	      ,(SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=gle.[DWNavisionCompany] AND [Department Code]='')) AS [CurrencyKey]
	,[DWNavisionCompany] + N'-' + [Department Code] AS [DepartmentKey]
	,[DWNavisionCompany] + N'-' + [G/L Account No.] AS [GlobalAccountKey]
	,CONVERT(NVARCHAR(20),gle.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),gle.[Reason Code]) AS CostChangeReasonKey
	,CONVERT(nvarchar(50),[DWNavisionCompany] + N'-'+ CONVERT(nvarchar(40),[Entry No.])) AS GLInfoKey
	,CASE
		WHEN (SELECT ga.[Income/Balance] FROM [EETDW_Extract].[nav].[G/L Account] ga WHERE ga.DWNavisionCompany = gle.[DWNavisionCompany] AND ga.[No.] = gle.[G/L Account No.]) = 0
			THEN [Amount] * -1
		ELSE [Amount]
	END AS Amount
FROM
	[EETDW_Extract].[nav].[G/L Entry] gle

UNION ALL

SELECT
	[Entry No.]
	,N'' AS DocumentKey
	,N'' AS [Document Type]
	,N'' AS [Document No]
	,[CalendarKey]
	,[CompanyKey]
	,[CurrencyKey]
	,N'' AS [DepartmentKey]
	,[GlobalAccountKey]
	,N'' AS CostChangeReasonKey
	,N'' AS GLInfoKey
	,[Amount]
FROM
	[EETDW_Extract].[control].[GL_OpeningBalance_2014]</property>
