                <component
                  refId="Package\SEQC Transform CustomerClassCount\DFT Transform CustomerClassCount\OLE SRC Extract"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  name="OLE SRC Extract"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH f AS
(SELECT
	
	c.MonthKey
	,s.[CustomerBillToKey]	
	,(SELECT
		count(distinct year(h.[CalendarKey]) * 100 + month(h.[CalendarKey]))
	
		FROM [EETDW_Stage].[dbo].[SalesItemLedgerEntry] AS h
	
		WHERE h.[CalendarKey] BETWEEN c.FirstDayOM AND c.LastDayOM 
			AND h.[CustomerBillToKey] = s.[CustomerBillToKey]
			AND LEFT(h.[OrderTypeKey],5) = '0-0-0') AS NumOfSales 
	
	
	,(SELECT
	count(distinct a.[Item Area Code])
	FROM [EETDW_Stage].[dbo].[SalesItemLedgerEntry] AS h
	INNER JOIN
	  (SELECT [Code] ,[Item Area Code] FROM [EETDW_Extract].[nav].[Gen_ Product Posting Group] WHERE [DWNavisionCompany]='EET Group') a ON h.GPPGCurrentKey=a.Code
	WHERE 1=1
	    AND h.[CalendarKey] BETWEEN DATEADD(month, -(SELECT Value FROM [EETDW_Extract].control.[Parameters] WHERE [TextLabel] = 'FrequencyWindow') + 1 , c.FirstDayOM) --DATEADD(month, -11, c.FirstDayOM) 
		AND c.LastDayOM--c.FirstDayOM AND c.LastDayOM 
		AND h.[CustomerBillToKey] = s.[CustomerBillToKey]
	    AND NOT (a.[Item Area Code]='' OR a.[Item Area Code] LIKE '%inactive')
	    AND LEFT(OrderTypeKey,5) = '0-0-0') AS NumOfDistinctGPPGs 
	,(SELECT
	count(distinct year(h.[CalendarKey]) * 100 + month(h.[CalendarKey]))
	FROM [EETDW_Stage].[dbo].[SalesItemLedgerEntry] AS h
	WHERE h.[CalendarKey] BETWEEN DATEADD(month, -(SELECT Value FROM [EETDW_Extract].control.[Parameters] WHERE [TextLabel] = 'FrequencyWindow') + 1 , c.FirstDayOM) --DATEADD(month, -11, c.FirstDayOM) 
		AND c.LastDayOM 
		AND h.[CustomerBillToKey] = s.[CustomerBillToKey]
		AND LEFT(h.[OrderTypeKey],5) = '0-0-0') AS NumOfSalesRolling 
	
	
	,(SELECT
	count(distinct year(h.[CalendarKey]) * 100 + month(h.[CalendarKey]))
	FROM (SELECT [CalendarKey],[CustomerBillToKey] FROM [EETDW_Stage].[dbo].[SalesItemLedgerEntry] UNION ALL SELECT CAST('2013-12-31' AS DATETIME) AS CalendarKey, [CustomerBillToKey] FROM [EETDW_Extract].[control].[Invoice_OpeningCustomer_2014]) AS h
	WHERE h.[CalendarKey] BETWEEN '1900-01-01' AND DATEADD(day,-1,c.FirstDayOM)
		AND h.[CustomerBillToKey] = s.[CustomerBillToKey]
		-- we cannot do this here AND h.[OrderTypeKey] = '0-0-0'
		) AS NumOfSalesAllPrevious 
	
	
	,(SELECT
	count(distinct year(h.[CalendarKey]) * 100 + month(h.[CalendarKey]))
	FROM [EETDW_Stage].[dbo].[SalesItemLedgerEntry] AS h
	WHERE h.[CalendarKey] BETWEEN DATEADD(month, -(SELECT Value FROM [EETDW_Extract].control.[Parameters] WHERE [TextLabel] = 'ChurnDefinitionPeriod') + 1 , c.FirstDayOM) -- DATEADD(month, -2, c.FirstDayOM) 
		AND c.LastDayOM 
		AND h.[CustomerBillToKey] = s.[CustomerBillToKey]
		AND LEFT(h.[OrderTypeKey],5) = '0-0-0') AS NumOfSalesChurnPeriod 
	
	
	,(SELECT
	count(distinct year(h.[CalendarKey]) * 100 + month(h.[CalendarKey]))
	FROM [EETDW_Stage].[dbo].[SalesItemLedgerEntry] AS h
	WHERE h.[CalendarKey] BETWEEN DATEADD(month, -(SELECT Value FROM [EETDW_Extract].control.[Parameters] WHERE [TextLabel] = 'ChurnDefinitionPeriod'), c.FirstDayOM) -- DATEADD(month, -3, c.FirstDayOM) 
		AND c.LastDayOM 
		AND h.[CustomerBillToKey] = s.[CustomerBillToKey]
		AND LEFT(h.[OrderTypeKey],5) = '0-0-0') AS NumOfSalesLostPeriod 	
   
   ,(SELECT
	count(distinct year(h.[CalendarKey]) * 100 + month(h.[CalendarKey]))
	FROM [EETDW_Stage].[dbo].[SalesItemLedgerEntry] AS h
	WHERE h.[CalendarKey] BETWEEN DATEADD(month, -(SELECT Value FROM [EETDW_Extract].control.[Parameters] WHERE [TextLabel] = 'ChurnDefinitionPeriod'), c.FirstDayOM) -- DATEADD(month, -3, c.FirstDayOM) 
		AND DATEADD(day,-1, c.FirstDayOM)
		AND h.[CustomerBillToKey] = s.[CustomerBillToKey]
		AND LEFT(h.[OrderTypeKey],5) = '0-0-0') AS NumOfSalesReWinPeriod 	

FROM 

(SELECT MIN(CalendarDate) AS FirstDayOM
		,MAX(EOMONTH(CalendarDate)) AS LastDayOM
		,YearKey 
		,MonthKey 
	  FROM [EETDW].dim.Calendar 
	  WHERE CalendarDate BETWEEN '2015-01-01' AND eomonth(dateadd(month, -1, GETDATE()))
	  GROUP BY YearKey, MonthKey)  as c

CROSS JOIN 

(SELECT DISTINCT CustomerBillToKey,CompanyKey FROM [EETDW_Stage].[dbo].[SalesItemLedgerEntry] 
                                 WHERE [CalendarKey] BETWEEN '2014-01-01' AND GETDATE()
								        AND LEFT([OrderTypeKey],5) = '0-0-0') AS s -- since we also need customers with sales 1 year back
	
--WHERE 
--s.CustomerBillToKey = 'EET Austria-+4312165585'
--s.CustomerBillToKey = 'EET DENMARK-48792080'

GROUP BY s.CustomerBillToKey, c.YearKey, c.MonthKey, c.FirstDayOM, c.LastDayOM

)

SELECT
	[MonthKey] AS MonthID
   ,CONVERT(nvarchar(51), [CustomerBillToKey]) AS [CustomerBillToKey]
   ,(CASE WHEN NumOfSalesAllPrevious=0 AND NumOfSalesRolling=0 THEN N'X'
        WHEN NumOfSalesAllPrevious=0 THEN N'N'
        WHEN NumOfSalesLostPeriod=0 THEN N'L'
        WHEN NumOfSalesChurnPeriod=0 THEN N'C'
        WHEN NumOfSalesReWinPeriod=0 THEN N'W'
	    ELSE N'R'
		END) + N'-' + CAST(NumOfDistinctGPPGs AS nvarchar) + N'-' + CAST(NumOfSalesRolling AS nvarchar) AS CustomerClassKey
   ,NumOfSalesRolling AS [NumOfSalesPeriods] -- [NumOfSales]
   ,[NumOfDistinctGPPGs]
   ,CASE WHEN NumOfSalesChurnPeriod = 0 AND NumOfSalesLostPeriod &gt; 0 THEN 1 ELSE 0 END AS NumOfChurnPeriods
   ,CASE WHEN NumOfSalesLostPeriod = 0 THEN 1 ELSE 0 END AS NumOfLostPeriods
   ,CASE WHEN NumOfSalesReWinPeriod = 0 AND NumOfSalesLostPeriod &gt; 0 THEN 1 ELSE 0 END AS NumOfWinbackPeriods
 --INTO dbo.[TempActiveCustomerCount]
 FROM f
 WHERE NOT (NumOfSalesAllPrevious=0 AND NumOfSalesRolling=0)
 ORDER BY CustomerBillToKey, MonthKey</property>
