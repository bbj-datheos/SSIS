                  refId="Package\SEQC Transform Purchase\DFT Transform Purchase\OLE SRC Extract"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  name="OLE SRC Extract"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT --TOP 100
	ISNULL((SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=ile.[DWNavisionCompany] AND [Department Code]=ile.[Department Code])
	,(SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=ile.[DWNavisionCompany] AND [Department Code]='')) AS [CompanyKey]
	,CONVERT(NVARCHAR(20),ile.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30),ile.[Item No.]) AS [ItemKey]
	,CONVERT(NVARCHAR(20),ile.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), ile.[Source No.]) AS [VendorKey]
	,ile.[Posting Date] AS CalendarKey
	,ile.[Order Date] AS CalendarOrderKey
	,CASE
		WHEN (ile.[Source Currency Code] = '' OR ile.[Source Currency Code] IS NULL)
		THEN ISNULL((SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=ile.[DWNavisionCompany] AND [Department Code]=ile.[Department Code])
                 ,(SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=ile.[DWNavisionCompany] AND [Department Code]=''))
		ELSE ile.[Source Currency Code]
	END AS [CurrencyKey]
	,ile.Amount AS PurchaseAmount
	,ile.[Freight costs (LCY)] AS CostFreightAmountLCY
	,ile.[Handling costs (LCY)] AS CostHandlingAmountLCY
	,CONVERT(numeric(38,18),CONVERT(numeric(38,18),ile.[Handling costs (LCY)]) * CONVERT(numeric(38,18),Exchange.ExchangeRate)) AS HandlingCostDKK ---SRG: 20200214
	,ile.[Insurance costs (LCY)] AS CostInsuranceAmountLCY
	,ile.[Obsolescence costs (LCY)] AS CostObsolescenceAmountLCY
	,ile.[Cost Posted to G/L] AS [CostPostedToG/LAmount]
	,ile.[Invoiced Quantity] AS QuantityInvoiced
	,ile.[Tax costs (LCY)] AS TaxCostAmountLCY -- added by SRG 20191002
	,ile.[Order No.] AS [OrderNo]-- added by bbj 20180626
	,CONVERT(NVARCHAR(30),ile.[Document No.]) AS InvoiceNo--end bbj
	,CONVERT(NVARCHAR(100),N'PLE-' + ile.[Document No.] + N'-' + CASE WHEN ile.[Order No.]='' THEN ile.[Document No.] + ile.[No. Series] ELSE ile.[Order No.] END) AS DocumentKey-- added by bbj 20180913
	,ile.[DWNavisionCompany]+N'-'+ile.[Owner] +N'-'+ ile.[Location Code] AS [InventoryTypeKey]--end bbj-- added by bbj 20181024
	,CONVERT(NVARCHAR(10), ile.[Gen. Prod. Posting Group]) AS GPPGHistoricalKey--end bbj
FROM
	nav.[Item Ledger Entry] AS ile
	LEFT JOIN nav.[Cost changed Reasoncodes] ccr 
	ON ccr.DWNavisionCompany = ile.DWNavisionCompany AND ccr.Code = ile.[Cost changed Reasoncode]
	LEFT JOIN (
		SELECT
			[Currency Code]
			,[Company Name]
			,CONVERT(nvarchar(4),DATEPART(YEAR,[Period])) + N'-' + RIGHT(N'0'+CONVERT(nvarchar(2),DATEPART(MONTH,[Period])),2) AS MonthKey
			,MAX(CONVERT(numeric(38,18),CONVERT(numeric(38,18),[Rel. Exchange Rate Amount])/CONVERT(numeric(38,18),rer.[Exchange Rate Amount]))) AS ExchangeRate
		FROM
			[EETDW_Extract].[navcom].[Reporting Exchange Rate] rer
			INNER JOIN [EETDW_Extract].[navcom].[Business Unit] bu
			ON (bu.Consolidate=1 AND [Currency Code]=bu.[Reporting Currency Code])
			OR (bu.Consolidate=1 AND [Currency Code]=LEFT(bu.[Reporting Currency Code],3) AND rer.[Exchange Rate Type] NOT IN (-1,2,3))
		WHERE
			[Exchange Rate Type] = 0
			AND CONVERT(date,[Period]) &gt; '2013-12-31'
		GROUP BY
			[Currency Code]
			,[Company Name]
			,CONVERT(nvarchar(4),DATEPART(YEAR,[Period])) + N'-' + RIGHT(N'0'+CONVERT(nvarchar(2),DATEPART(MONTH,[Period])),2)
	) AS Exchange
	ON [Company Name] = ile.DWNavisionCompany
	AND CONVERT(nvarchar(4),DATEPART(YEAR,ile.[Posting Date])) + N'-' + RIGHT(N'0'+CONVERT(nvarchar(2),DATEPART(MONTH,ile.[Posting Date])),2) = MonthKey
WHERE
	(ile.[Entry Type] = 0) AND (ile.[Invoiced Quantity] &lt;&gt; 0) AND (ile.[Posting Date] &gt;= CONVERT(datetime, '2008-07-01 00:00:00', 102))</property>
