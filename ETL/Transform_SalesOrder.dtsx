                <component
                  refId="Package\SEQC Transform SalesOrder\DFT Transform SalesOrder\OLE SRC Extract"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  name="OLE SRC Extract"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT --TOP 1000
	ISNULL(
		(
			SELECT MAX([Code])
			FROM [EETDW_Extract].[navcom].[Business Unit]
			WHERE [Consolidate]=1 AND [Company Name]=sl.[DWNavisionCompany]	AND [Department Code]=sl.[Department Code]
		)
		,(
			SELECT MAX([Code])
			FROM [EETDW_Extract].[navcom].[Business Unit]
			WHERE [Consolidate]=1 AND [Company Name]=sl.[DWNavisionCompany] AND [Department Code]=''
		)
	) AS [CompanyKey]
	,CONVERT(NVARCHAR(20),sl.[Document No.]) AS SalesNo
	,CONVERT(NVARCHAR(30), sl.[Line No.]) AS SalesLineNo
	,CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sl.[Sell-to Customer No.]) AS [CustomerSellToKey]
	,CASE 
		WHEN LEN(cust.[Bill-to Customer No.]) &gt; 0 THEN CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), cust.[Bill-to Customer No.])
		ELSE CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sl.[Sell-to Customer No.]) 
	 END AS [CustomerBillToKey]
	,CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CASE WHEN sl.[Type]=3 THEN N'RES_' ELSE N'' END + CONVERT(NVARCHAR(50), sl.[No.]) AS ItemKey
	,sl.[Order Date] AS CalendarKey
	,sl.[Order Date] AS CalendarOrderKey
	,ISNULL(IIF(sl.[Shipment Date]&gt;ISNULL(shiplog.ShippingCalendarKey,'1900-01-01'),sl.[Shipment Date],shiplog.ShippingCalendarKey),'1900-01-01') AS CalendarShippingKey -- CHANGED SRG: 20200408
	,CONVERT(NVARCHAR(20),sh.[Origin of order]) AS OriginOfOrderKey
	,CASE
		WHEN (sl.[Currency Code] = '' OR sl.[Currency Code] IS NULL)
		THEN 
		  CASE WHEN (sh.[Currency Code] = '' OR sh.[Currency Code] IS NULL)
		  THEN ISNULL((SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=sh.[DWNavisionCompany] AND [Department Code]=sh.[Department Code])
	                 ,(SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=sh.[DWNavisionCompany] AND [Department Code]=''))
		  ELSE sh.[Currency Code]
		END  
		  ELSE sl.[Currency Code]
	 END AS [CurrencyKey]
	,CONVERT(nvarchar(10),
		CASE 
			WHEN LEN(sl.[RMA No]) &gt; 0 THEN N'1-0-0-' + ISNULL(CONVERT(nvarchar(10),ReasonCode.[No.]),N'0')-- added by BBJ 20181214 to support new blanket order type
			WHEN sl.[Document Type]=4 THEN N'0-1-0-0' -- end bbj	
			ELSE N'0-0-0-0' 
		END
	) AS [OrderTypeKey]
	,CASE WHEN ISNULL(sl.[Bid No],N'')='' THEN CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sl.[Cost changed Reasoncode])
	        ELSE CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sl.[Cost changed Reasoncode]) + N'-' + CONVERT(NVARCHAR(30),sl.[Bid No]) END AS CostChangeReasonKey
	,CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sh.[Salesperson Code]) AS EmployeeKey
	,CONVERT(INT,pn.ID) AS PersonKey
	,CONVERT(NVARCHAR(20),sh.[Order Status]) + N'-' + CONVERT(NVARCHAR(20),sh.[Hold invoice]) AS OrderStatusKey
	,CONVERT(INT,ShippingOrderStatus.ShippingOrderStatusKey) AS ShippingOrderStatusKey
	,CASE 
	      WHEN sl.[Qty. Shipped Not Invoiced]&lt;&gt;0 AND sl.Quantity&lt;&gt;0 THEN (sl.[Amount (LCY)]*sl.[Qty. Shipped Not Invoiced]/sl.Quantity)
		  WHEN sl.[Qty. Shipped Not Invoiced]=0 AND sl.[Outstanding Quantity]&lt;&gt;0 AND sl.Quantity&lt;&gt;0 THEN (sl.[Amount (LCY)]*sl.[Outstanding Quantity]/sl.Quantity)
	         ELSE 0 END AS SalesAmount -- 20180307 added by bbj after agreement with Jan Iversen
	,CASE 
	      WHEN sl.[Qty. Shipped Not Invoiced]&lt;&gt;0 AND sl.Quantity&lt;&gt;0 THEN sl.[Order Cost Amount (LCY)]*-1*sl.[Qty. Shipped Not Invoiced]/sl.Quantity
		  WHEN sl.[Qty. Shipped Not Invoiced]=0 AND sl.[Outstanding Quantity]&lt;&gt;0 AND sl.Quantity&lt;&gt;0 THEN sl.[Order Cost Amount (LCY)]*-1*sl.[Outstanding Quantity]/sl.Quantity
	         ELSE 0 END AS OrderCostAmount -- Sales Cost
	,CASE 
	      WHEN sl.[Qty. Shipped Not Invoiced]&lt;&gt;0 AND sl.Quantity&lt;&gt;0 THEN (sl.[Order Profit Margin (LCY)]*sl.[Qty. Shipped Not Invoiced]/sl.Quantity)
		  WHEN sl.[Qty. Shipped Not Invoiced]=0 AND sl.[Outstanding Quantity]&lt;&gt;0 AND sl.Quantity&lt;&gt;0 THEN (sl.[Order Profit Margin (LCY)]*sl.[Outstanding Quantity]/sl.Quantity)
	         ELSE 0 END AS OrderProfitMarginAmount -- Sales Profit
	,CASE 
	      WHEN sl.[Qty. Shipped Not Invoiced]&lt;&gt;0 AND sl.Quantity&lt;&gt;0 THEN sl.[Unit Cost (LCY)]*-1*sl.[Qty. Shipped Not Invoiced]/sl.Quantity
		  WHEN sl.[Qty. Shipped Not Invoiced]=0 AND sl.[Outstanding Quantity]&lt;&gt;0 AND sl.Quantity&lt;&gt;0 THEN sl.[Unit Cost (LCY)]*-1*sl.[Outstanding Quantity]/sl.Quantity
	        ELSE sl.[Unit Cost (LCY)]*-1 END AS UnitCostAmount -- Financial cost
	,sl.[Outstanding Quantity] AS SalesOrderQuantity -- added by bbj 20180828 ---- changed Name by SRG: 20191213
	,CONVERT(int,[Qty. Shipped Not Invoiced]+[Outstanding Quantity]) AS SalesOrderQuantityNew ---- SRG: 20200131
	,CONVERT(int,[Qty. Shipped Not Invoiced]) AS SalesOrderShippedNotInvoicedQuantity---- SRG: 20200131
	,CONVERT(int,[Outstanding Quantity]) AS SalesOrderOutstandingQuantity---- SRG: 20200131
	,CONVERT(numeric(18,6),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0
			ELSE ([Amount (LCY)]/Quantity)*([Qty. Shipped Not Invoiced]+[Outstanding Quantity])
		END
	) AS SalesOrderTurnoverNew---- SRG: 20200131
	,CONVERT(numeric(38,18),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0.0
			ELSE (CONVERT(numeric(38,18),[Amount (LCY)])/CONVERT(numeric(38,18),Quantity))*(CONVERT(numeric(38,18),[Qty. Shipped Not Invoiced])+CONVERT(numeric(38,18),[Outstanding Quantity])) * CONVERT(numeric(38,18),Exchange.ExchangeRate)
		END
	) AS SalesOrderTurnoverDKK---- SRG: 20200207
	,CONVERT(numeric(18,6),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0
			ELSE ([Amount (LCY)]/Quantity)*([Qty. Shipped Not Invoiced]+[Outstanding Quantity])
		END
	) AS SalesOrderTurnoverLCY---- SRG: 20200207
	,CONVERT(numeric(18,6),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0
			ELSE ([Amount (LCY)]/Quantity)*[Qty. Shipped Not Invoiced]
		END
	) AS SalesOrderShippedNotInvoicedTurnover---- SRG: 20200131
	,CONVERT(numeric(18,6),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0
			ELSE ([Amount (LCY)]/Quantity)*[Outstanding Quantity]
		END
	)AS SalesOrderOutstandingTurnover---- SRG: 20200131
	,CONVERT(numeric(18,6),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0
			ELSE ([Amount (LCY)]/Quantity)*([Qty. Shipped Not Invoiced]+[Outstanding Quantity])*([Profit %]/100)
		END
	) AS SalesOrderProfitNew---- SRG: 20200131
	,CONVERT(numeric(38,18),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0.0
			ELSE (CONVERT(numeric(38,18),[Amount (LCY)])/CONVERT(numeric(38,18),Quantity))*(CONVERT(numeric(38,18),[Qty. Shipped Not Invoiced])+CONVERT(numeric(38,18),[Outstanding Quantity]))*(CONVERT(numeric(38,18),[Profit %])/100) * CONVERT(numeric(38,18),Exchange.ExchangeRate)
		END
	) AS SalesOrderProfitDKK---- SRG: 20200207
	,CONVERT(numeric(18,6),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0
			ELSE ([Amount (LCY)]/Quantity)*([Qty. Shipped Not Invoiced]+[Outstanding Quantity])*([Profit %]/100)
		END
	) AS SalesOrderProfitLCY---- SRG: 20200207
	,CONVERT(numeric(18,6),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0
			ELSE ([Amount (LCY)]/Quantity)*[Qty. Shipped Not Invoiced]*([Profit %]/100)
		END
	) AS SalesOrderShippedNotInvoicedProfit---- SRG: 20200131
	,CONVERT(numeric(18,6),
		CASE
			WHEN [Amount (LCY)] = 0 THEN 0
			ELSE ([Amount (LCY)]/Quantity)*[Outstanding Quantity]*([Profit %]/100)
		END
	) AS SalesOrderOutstandingProfit---- SRG: 20200131


	,CONVERT(NVARCHAR(100),N'SO-' + sh.[No.] + N'-' + ISNULL(sh.[Org. Sales Order No.],N'')) AS DocumentKey --end bbj
	,CONVERT(nvarchar(50),sl.[DWNavisionCompany]+N'-'+sl.[Owner] +N'-'+ sl.[Location Code]) AS [InventoryTypeKey] 	-- added by bbj 20180913 -- changed SRG 20191031
FROM
	nav.[Sales Line] AS sl
	LEFT JOIN nav.Customer AS cust ON cust.[No.] = sl.[Sell-to Customer No.] AND cust.DWNavisionCompany = sl.DWNavisionCompany
	LEFT JOIN nav.[Sales Header] AS sh ON sh.[No.] = sl.[Document No.] AND sh.DWNavisionCompany = sl.DWNavisionCompany 
	LEFT JOIN nav.[Cost changed Reasoncodes] ccr ON ccr.DWNavisionCompany = sl.DWNavisionCompany AND ccr.Code = sl.[Cost changed Reasoncode]
	LEFT JOIN EETDW_Stage.map.PersonNavision pn on pn.SystemUser = sh.[Salesperson Code]
	LEFT JOIN (
		SELECT DISTINCT
			CONVERT(nvarchar(10),[No.]) AS [No.]
		FROM
			[nav].[RMA Reasoncode]
		WHERE
			[DWNavisionCompany] = 'EET Group'
	) AS ReasonCode
	ON ReasonCode.[No.] = sh.[Reason Code]
	--Added to only show the english reason code, as the dimension is kind of handheld.
	--If it becomes more dynamic, the where should be removed.
	--ADDED by SRG 20191008
	LEFT JOIN (
		SELECT
			 [Order No.]
			,[Line No.]
			,MAX(CONVERT(date,[New Shipment Date (Order)])) AS ShippingCalendarKey
			,[DWNavisionCompany]
		FROM
			[nav].[Shipment/Receipt Date Log]
		WHERE
			Active = '1'
			AND [Type] = 2
			AND CONVERT(date,[New Shipment Date (Order)]) &lt;&gt; '1753-01-01'
		GROUP BY
			 [Order No.]
			,[Line No.]
			,[DWNavisionCompany]
	) AS ShipLog ---- SRG: 20191213
	ON sl.[Document No.] = ShipLog.[Order No.]
	AND sl.[Line No.] = ShipLog.[Line No.]
	AND sl.DWNavisionCompany = ShipLog.DWNavisionCompany
	LEFT JOIN (
		SELECT
			 [Order No.]
			,[Line No.]
			,MAX([Status]) AS ShippingOrderStatusKey
			,[DWNavisionCompany]
		FROM
			[nav].[Shipment/Receipt Date Log]
		WHERE
			Active = '1'
			AND [Type] = 2
		GROUP BY
			 [Order No.]
			,[Line No.]
			,[DWNavisionCompany]
	) AS ShippingOrderStatus ---- SRG: 20200204
	ON sl.[Document No.] = ShippingOrderStatus.[Order No.]
	AND sl.[Line No.] = ShippingOrderStatus.[Line No.]
	AND sl.DWNavisionCompany = ShippingOrderStatus.DWNavisionCompany
	LEFT JOIN (
		SELECT
			 [Currency Code]
			,[Company Name]
			,CONVERT(nvarchar(4),DATEPART(YEAR,[Period])) + N'-' + RIGHT(N'0'+CONVERT(nvarchar(2),DATEPART(MONTH,[Period])),2) AS MonthKey
			,MAX(CONVERT(numeric(38,18),CONVERT(numeric(38,18),[Rel. Exchange Rate Amount])/CONVERT(numeric(38,18),rer.[Exchange Rate Amount]))) AS ExchangeRate
		FROM
			[EETDW_Extract].[navcom].[Reporting Exchange Rate] rer
			INNER JOIN [EETDW_Extract].[navcom].[Business Unit] bu
			ON (bu.Consolidate=1 AND [Currency Code]=bu.[Reporting Currency Code])
			OR (bu.Consolidate=1 AND [Currency Code]=LEFT(bu.[Reporting Currency Code],3) AND rer.[Exchange Rate Type] NOT IN (-1,2,3))
		WHERE
			[Exchange Rate Type] = 0
			AND CONVERT(date,[Period]) &gt; '2013-12-31'
		GROUP BY
			[Currency Code]
			,[Company Name]
			,CONVERT(nvarchar(4),DATEPART(YEAR,[Period])) + N'-' + RIGHT(N'0'+CONVERT(nvarchar(2),DATEPART(MONTH,[Period])),2)
	) AS Exchange
	ON [Company Name] = sl.DWNavisionCompany
	AND CONVERT(nvarchar(4),DATEPART(YEAR,GETDATE()))+'-'+RIGHT(N'0'+CONVERT(nvarchar(2),DATEPART(MONTH,GETDATE())),2) = MonthKey


WHERE
	sl.[Document Type] IN (1,4)  --- sl.[Document Type] = 1  changed by bbj 20180928 due to new blanket order
	and sl.[Type] &lt;&gt; ''
	and sl.[No.]&lt;&gt;''
	and not (sl.[Document Type] =1 AND sl.[Blanket Order No.]&lt;&gt;'')    ---changed by bbj 20180928 due to new blanket order
	and (sl.[Outstanding Quantity]&lt;&gt;0 OR sl.[Qty. Shipped Not Invoiced]&lt;&gt;0)</property>
