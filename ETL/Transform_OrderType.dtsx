                  name="OLE SRC Extract"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	 CONVERT(nvarchar(10),OrderTypeKey + N'-' + ISNULL([No.],N'0')) AS OrderTypeKey 
	,CONVERT(nvarchar(10),OrderTypeName)							AS OrderTypeName
	,CONVERT(nvarchar(10),OrderTypeDropShipment)					AS OrderTypeDropShipment
	,CONVERT(nvarchar(50),ISNULL(B.[Name],N'?'))					AS OrderTypeRMAReason
	,CONVERT(nvarchar(10),ISNULL([No.],N'0'))						AS OrderTypeRMAReasonCode
FROM (
	SELECT 
		 CONVERT(NVARCHAR(23),N'0-0-0') AS OrderTypeKey
		,CONVERT(NVARCHAR(23),N'Standard') AS OrderTypeName
		,CONVERT(NVARCHAR(23),N'No') AS OrderTypeDropShipment
	
	UNION ALL
	
	SELECT 
		 CONVERT(NVARCHAR(23),N'0-0-1') AS OrderTypeKey
		,CONVERT(NVARCHAR(23),N'Standard') AS OrderTypeName
		,CONVERT(NVARCHAR(23),N'Yes') AS OrderTypeDropShipment
	
	UNION ALL
	
	SELECT 
		 CONVERT(NVARCHAR(23),N'0-1-0') AS OrderTypeKey
		,CONVERT(NVARCHAR(23),N'Blanket') AS OrderTypeName
		,CONVERT(NVARCHAR(23),N'No') AS OrderTypeDropShipment
	
	UNION ALL
	
	SELECT 
		 CONVERT(NVARCHAR(23),N'1-0-0')  AS OrderTypeKey
		,CONVERT(NVARCHAR(23),N'RMA') AS OrderTypeName
		,CONVERT(NVARCHAR(23),N'No') AS OrderTypeDropShipment
	
	UNION ALL
	
	SELECT 
		 CONVERT(NVARCHAR(23),N'1-0-1')  AS OrderTypeKey
		,CONVERT(NVARCHAR(23),N'RMA') AS OrderTypeName
		,CONVERT(NVARCHAR(23),N'Yes') AS OrderTypeDropShipment
	
	UNION ALL
	
	SELECT
		 CONVERT(NVARCHAR(23),N'1-1-1')  AS OrderTypeKey
		,CONVERT(NVARCHAR(23),N'Resource') AS OrderTypeName
		,CONVERT(NVARCHAR(23),N'No') AS OrderTypeDropShipment
) A
LEFT JOIN (
	SELECT---To include the RMA without a Reason code
		 N'0' AS [No.]
		,N'?' AS [NAME]
		
	UNION
	
	SELECT
		CONVERT(nvarchar(10),[No.]) AS [No.]
		,[NAME]
	FROM
		[nav].[RMA Reasoncode]
	WHERE
		[DWNavisionCompany] = 'EET Group'
) B
ON OrderTypeName = 'RMA'



/*
SELECT 
	   '1-' + CAST(t.[Code] AS NVARCHAR(10)) + N'-' + CAST(s.[Code] AS NVARCHAR(10)) AS [OrderTypeKey]
	  ,'1-' + CAST(t.[Code] AS NVARCHAR(10)) + N'-' + CAST(s.[Code] AS NVARCHAR(10)) AS [OrderTypeName]
      ,t.[Name] AS [OrderTypeRMATypeCode]
      ,t.[Name] AS [OrderTypeRMATypeName]
	  ,s.[Description] as [OrderTypeRMAStatusCode]
	  ,s.[Description] as [OrderTypeRMAStatusName]
FROM [EETDW_Extract].[control].[EET RMA Types] as t -- use [control].[EET RMA Types] from Extract layer
CROSS JOIN  [EETDW_Extract].[control].[EET RMA Status] AS s -- use [control].[EET RMA Status] from Extract layer
UNION
SELECT '0-0-0', '', '', '', '', ''
*/</property>
