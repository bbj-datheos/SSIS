                <component
                  refId="Package\SEQC Transform Vendor\DFT Transform Vendor\OLE SRC Extract"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  name="OLE SRC Extract"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	CONVERT(NVARCHAR(20),[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30),REPLACE(REPLACE([No.],CHAR(10),'NL'),CHAR(13),'CR')) as [VendorKey]
	,CONVERT(NVARCHAR(20),[DWNavisionCompany]) AS CompanyCode
	,CONVERT(NVARCHAR(30),v.[Name])AS VendorName
	,CONVERT(NVARCHAR(10),[Gen. Bus. Posting Group])AS VendorGeneralBusinessPostingGroupCode
	,CONVERT(NVARCHAR(10),[Vendor Posting Group])AS VendorPostingGroupCode
	,CONVERT(NVARCHAR(30),[Territory Code])AS VendorTerritoryCode
	,CONVERT(NVARCHAR(30),v.[Business Unit])AS VendorBusinessUnitCode
	,CONVERT(NVARCHAR(30),[Global Business Partners]) AS VendorGlobalBusinessPartnersCode
	,CONVERT(NVARCHAR(30),REPLACE(REPLACE([Address],CHAR(10),''),CHAR(13),'')) AS VendorAddress
	,CONVERT(NVARCHAR(30),[Post Code])AS VendorPostCode
	,CONVERT(NVARCHAR(30),REPLACE(REPLACE([City],CHAR(10),''),CHAR(13),'')) AS VendorCityCode
	,CONVERT(NVARCHAR(10),
		CASE 
			WHEN ([Country Code] = '' AND co.[Company Country Code] &lt;&gt; 'G1') THEN co.[Company Country Code]
			WHEN ([Country Code] = '' AND co.[Company Country Code] = 'G1') THEN 'DK'
			ELSE [Country Code]  
		END) AS VendorCountryCode
		,[Currency Code]
	,CONVERT(NVARCHAR(10),
		CASE 
			WHEN ([Currency Code] = '' OR [Currency Code] IS NULL) 
			THEN ISNULL((SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=v.[DWNavisionCompany] AND [Department Code]=v.[Department Code])
					   ,(SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=v.[DWNavisionCompany] AND [Department Code]='')) 
			ELSE [Currency Code] END) AS VendorCurrencyCode
	,CONVERT(NVARCHAR(20), [VAT Registration No.])AS VendorVatNo
	,CONVERT(NVARCHAR(20), [Fiscal Code])AS VendorFiscalCode
	,CONVERT(NVARCHAR(30),[Payment Terms Code])AS VendorPaymentTermsCode
	,CONVERT(NVARCHAR(30), [Shipment Method Code])AS VendorShipmentMethodCode
	,CONVERT(NVARCHAR(30), [Delivery Terms Code])AS VendorDeliveryTermsCode
	,CONVERT(NVARCHAR(30), [Purchaser Code]) AS VendorPurchaserCode
	,CONVERT(NVARCHAR(30), [Accounting Responsible]) AS VendorAccountingResponsibleCode
	,CONVERT(INT,Blocked)AS VendorBlockingIndicator  
  	,CAST([Contract Date] AS Date) AS VendorContractDate
	,CONVERT(NVARCHAR(10), [Stock Rotation]) AS VendorStockRotation
	,CONVERT(INT,[Performance Bonus])AS VendorPerformanceBonusIndicator  
	,CONVERT(NVARCHAR(10), [MDF]) AS VendorMDFCode
	-- added by bbj 20180615
,(SELECT [Avg Credit Days] FROM [EETDW_Extract].[nav].[Payment Terms] WHERE [Code]=v.[Payment Terms Code] AND [DWNavisionCompany]=v.[DWNavisionCompany]) AS [AvgCreditDays]
--end bbj

  FROM [EETDW_Extract].[nav].[Vendor] v
  LEFT JOIN [EETDW_Extract].[navcom].[Business Unit] bu ON v.DWNavisionCompany = bu.[Company Name] and v.[Department Code] = bu.[Department Code]
LEFT JOIN [EETDW_Extract].[navcom].[Company] co ON v.DWNavisionCompany = co.Name</property>
