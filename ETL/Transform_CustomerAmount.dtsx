                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/****** Script for SelectTopNRows command from SSMS  ******/
SELECT 

ISNULL((SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=c.[DWNavisionCompany] AND [Department Code]=[Department Code])
	   ,(SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=c.[DWNavisionCompany] AND [Department Code]='')) AS [CompanyKey]

,CONVERT(NVARCHAR(20),c.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), c.[No.]) AS [CustomerBillToKey]

,CONVERT(NVARCHAR(20),c.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), c.[No.]) AS [CustomerSellToKey]
,ISNULL((SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=c.[DWNavisionCompany] AND [Department Code]=[Department Code])
	   ,(SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=c.[DWNavisionCompany] AND [Department Code]='')) AS [CurrencyKey]


     
      ,[Credit Limit (LCY)] AS [CreditLimitAmount]
     
      ,[Insurance Amount (LCY)] AS [InsuranceAmount]
,[DWNavisionCompany]
--,GetDate() AS DWCreatedDate
--INTO [EETDW_Stage].[dbo].[CustomerAmount]
  FROM [EETDW_Extract].[nav].[Customer] c
  WHERE [Credit Limit (LCY)]&lt;&gt;0 OR [Insurance Amount (LCY)]&lt;&gt;0</property>
