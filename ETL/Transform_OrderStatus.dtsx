                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT
	 CONVERT(NVARCHAR(20),sh.[Order Status]) + N'-' + CONVERT(NVARCHAR(20),sh.[Hold invoice]) AS OrderStatusKey
	,CONVERT(nvarchar(50),
		CASE CONVERT(nvarchar(2),sh.[Order Status])
			WHEN N'0' THEN N'Adding'
			WHEN N'1' THEN N'Ready for Picking'
			WHEN N'2' THEN N'Picking in Progress'
			WHEN N'3' THEN N'Update in Process'
		END
		+ N' - '+
		CASE CONVERT(nvarchar(2),sh.[Hold invoice])
			WHEN N'0' THEN N'Not on Hold'
			WHEN N'1' THEN N'On Hold'
		END
	) AS OrderStatusName
	,CONVERT(nvarchar(5),sh.[Order Status]) AS [OrderStatusStatusCode]
	,CONVERT(nvarchar(20),
		CASE CONVERT(nvarchar(2),sh.[Order Status])
			WHEN N'0' THEN N'Adding'
			WHEN N'1' THEN N'Ready for Picking'
			WHEN N'2' THEN N'Picking in Progress'
			WHEN N'3' THEN N'Update in Process'
		END
	) AS [OrderStatusStatusName]
	,CONVERT(nvarchar(5),sh.[Hold invoice]) AS [OrderStatusHoldInvoiceCode]
	,CONVERT(nvarchar(5),
		CASE CONVERT(nvarchar(2),sh.[Hold invoice])
			WHEN N'0' THEN N'No'
			WHEN N'1' THEN N'Yes'
		END
	) AS [OrderStatusHoldInvoiceName]
FROM
	nav.[Sales Header] sh</property>
