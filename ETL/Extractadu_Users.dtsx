                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">with extracted as (
	select 
	   [SID]
      ,[Name]
      ,[Initials]
      ,[Username]
      ,[Enabled]
      ,[EmailAddress]
	  ,replace(
			[SecondaryEmailAddresses], 
			'smtp:', 
			'$') as [SecondaryEmailAddresses]
	  ,[Title]
      ,[Department]
      ,[Company]
      ,[Manager]
      ,[Phone]
      ,[Mobile]
      ,[SkypeEnabled]
      ,[City]
      ,[StreetAddress]
      ,[PostalCode]
      ,[Country]
      ,[eetAccountType]
      ,[eetDepartment]
      ,[eetJobFunction]
      ,[eetWorkArea]
      ,[eetLocation]
      ,[eetRID]
	from [AD_Users].[dw].[ALL_UserInformations]
), split as (
	select
		[SID]
      ,[Name]
      ,[Initials]
      ,[Username]
      ,[Enabled]
      ,[EmailAddress]
	  ,LTRIM(RTRIM(m.n.value('.[1]','varchar(8000)'))) as SecondaryEmailAddresses
	  ,[Title]
      ,[Department]
      ,[Company]
      ,[Manager]
      ,[Phone]
      ,[Mobile]
      ,[SkypeEnabled]
      ,[City]
      ,[StreetAddress]
      ,[PostalCode]
      ,[Country]
      ,[eetAccountType]
      ,[eetDepartment]
      ,[eetJobFunction]
      ,[eetWorkArea]
      ,[eetLocation]
      ,[eetRID]
	from
		(
		select 
			 [SID]
			,[Name]
			,[Initials]
			,[Username]
			,[Enabled]
			,[EmailAddress]
			,cast('&lt;XMLRoot&gt;&lt;RowData&gt;' + replace(SecondaryEmailAddresses,'$','&lt;/RowData&gt;&lt;RowData&gt;') + '&lt;/RowData&gt;&lt;/XMLRoot&gt;' as XML) as x
			,[Title]
			,[Department]
			,[Company]
			,[Manager]
			,[Phone]
			,[Mobile]
			,[SkypeEnabled]
			,[City]
			,[StreetAddress]
			,[PostalCode]
			,[Country]
			,[eetAccountType]
			,[eetDepartment]
			,[eetJobFunction]
			,[eetWorkArea]
			,[eetLocation]
			,[eetRID]
		from extracted
		) t
		CROSS APPLY x.nodes('/XMLRoot/RowData')m(n)
	), mainCTE as (
	select
		left(SecondaryEmailAddresses, 
			case 
				when charindex(' ', SecondaryEmailAddresses) = 0 then len (SecondaryEmailAddresses) 
				else charindex(' ', SecondaryEmailAddresses) - 1 
			end
			) as SecondaryEmailAddress
		,*
	from split
	)

	select distinct
			 [SID]
			,[Name]
			,[Initials]
			,[Username]
			,[Enabled]
			,[EmailAddress]
			,convert(nvarchar,case 
				when SecondaryEmailAddress like 'X400%' or SecondaryEmailAddress like 'Not%' then ''
				when EmailAddress = SecondaryEmailAddress then ''
				else SecondaryEmailAddress
