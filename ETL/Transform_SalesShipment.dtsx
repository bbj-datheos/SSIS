                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
--TOP 100000
ISNULL((SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=sasl.[DWNavisionCompany] AND [Department Code]=sasl.[Department Code])
	   ,(SELECT MAX([Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=sasl.[DWNavisionCompany] AND [Department Code]='')) AS [CompanyKey]
,CONVERT(NVARCHAR(20),sasl.[Document No.]) AS SalesShipmentNo
,CONVERT(NVARCHAR(30), sasl.[Line No.]) AS SalesShipmentLineNo
,CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sasl.[Sell-to Customer No.]) AS [CustomerSellToKey]
,CASE 
	WHEN LEN(cust.[Bill-to Customer No.]) &gt; 0 
		THEN CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), cust.[Bill-to Customer No.])
		ELSE CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sasl.[Sell-to Customer No.]) 
 END AS [CustomerBillToKey]
,CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(50), sasl.[No.]) AS ItemKey
,ssh.[Posting Date] AS CalendarKey
--,sasl.[Shipment Date] AS CalendarKey
,ssh.[Order Date] AS CalendarOrderKey
,sasl.[Shipment Date] AS CalendarShipmentKey

,CONVERT(NVARCHAR(100),N'SHP-' + sasl.[Document No.] + N'-' + CASE WHEN ISNULL(ssh.[Order No.],N'')='' THEN sasl.[Document No.] + N'' ELSE ssh.[Order No.] END  + N'-' + ISNULL(N'',N'')) AS DocumentKey

,CASE 
	WHEN LEN(ssh.[RMA No.]) &gt; 0 AND ssh.[Drop Shipment] &lt;&gt; 0 THEN N'1-0-0' 
	WHEN LEN(ssh.[RMA No.]) &gt; 0 AND ssh.[Drop Shipment] = 0 THEN N'1-0-1' 
    WHEN LEN(ssh.[RMA No.]) = 0 AND ssh.[Drop Shipment] &lt;&gt; 0 THEN N'0-0-1' 	
 		ELSE N'0-0-0' 
 END AS [OrderTypeKey]
--,CONVERT(NVARCHAR(20),ssh.[Origin of order]) AS OriginOfOrderKey
--,CASE
--	WHEN sasl.[Currency Code] = ''
--	THEN N'DKK'
--	ELSE CONVERT(NVARCHAR(10), sl.[Currency Code]) 
-- END AS CurrencyKey
--,CASE 
--	WHEN LEN(ssh.[RMA No.]) &gt; 0 
--		THEN N'1-0-0' 
--		ELSE N'0-0-0' 
-- END AS [OrderTypeKey]
--,CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sasl.[Cost changed Reasoncode]) AS CostChangeReasonKey
--,CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sash.[Salesperson Code]) AS EmployeeKey


--,(sl.[Amount (LCY)]*-1) AS SalesAmount -- Revenue
--,sl.[Order Cost Amount (LCY)] AS OrderCostAmount -- Sales Cost
--,(sl.[Order Profit Margin (LCY)] * -1) AS OrderProfitMarginAmount -- Sales Profit
--,sl.[Unit Cost (LCY)] AS UnitCostAmount -- Financial cost
--,CASE 
--	WHEN ccr.[Cost changed Type] = 1 
--	THEN ((ile.Amount - ile.[Cost Posted to G/L] - ile.[Order Cost Change Amount]) * -1)
--	ELSE ((ile.Amount - ile.[Cost Posted to G/L]) * -1)
--	END AS [MarginPostedToG/LAmount]	
--,sl.[Order Cost Change Amount] AS OrderCostChangeAmount
--,(sl.[Invoiced Quantity] * -1) AS [InvoicedQuantity]
--,ile.[Cost Posted to G/L] AS [CostPostedToG/LAmount]


-- changed by bbj 20181122
 --,ssh.[Handling Fee (LCY)] AS HandlingFeeAmount1
 --,ssh.[Logistical Unit] AS LogisticalUnit1
 --,sasl.[Logistical Unit Factor]
 --,(0.6/countlines.NoOfSL + 0.38 + 0.02*CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END*sasl.Quantity) AS A

 -- changed again by bbj 20190111
 --,CASE WHEN ssh.[Logistical Unit] = 0 THEN 0 ELSE ssh.[Handling Fee (LCY)]*(0.6/countlines.NoOfSL + 0.38* CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END + 0.02*CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END*ABS(sasl.Quantity))/ssh.[Logistical Unit] END  AS HandlingFeeAmount
 -- ,CASE WHEN ssh.[Logistical Unit] = 0 THEN 0 ELSE ssh.[Actual freight cost (LCY)]*(0.6/countlines.NoOfSL + 0.38* CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END + 0.02*CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END*ABS(sasl.Quantity))/ssh.[Logistical Unit] END  AS ActualFreightCostAmount
 --,0.6/countlines.NoOfSL + 0.38* CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END + 0.02*CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END*ABS(sasl.Quantity) AS LogisticalUnit


 ,CASE WHEN ssh.[Logistical Unit] = 0 OR [Non-physical item] &lt;&gt; 0 OR [Drop Shipment] &lt;&gt; 0 THEN 0 ELSE ssh.[Handling Fee (LCY)]*(0.6/countlines.NoOfSL + 0.38* CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END + 0.02*CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END*ABS(sasl.Quantity))/ssh.[Logistical Unit] END  AS HandlingFeeAmount
  ,CASE WHEN ssh.[Logistical Unit] = 0 OR [Non-physical item] &lt;&gt; 0  OR [Drop Shipment] &lt;&gt; 0 THEN 0 ELSE ssh.[Actual freight cost (LCY)]*(0.6/countlines.NoOfSL + 0.38* CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END + 0.02*CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END*ABS(sasl.Quantity))/ssh.[Logistical Unit] END  AS ActualFreightCostAmount
 ,(CASE WHEN [Non-physical item]=0 AND [Drop Shipment]=0 THEN 1 ELSE 0 END)*(0.6/countlines.NoOfSL + 0.38* CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END + 0.02*CASE WHEN sasl.[Logistical Unit Factor] &gt;0 THEN sasl.[Logistical Unit Factor] ELSE 1 END*ABS(sasl.Quantity)) AS LogisticalUnit
-- ,ssh.[Logistical Unit] 

 ,sasl.[Gross Weight] AS GrossWeight
 ,sasl.[Net Weight] AS NetWeight
 ,sasl.Quantity AS ShipmentQuantity
 ,CASE 
	WHEN i.[Unit Volume] IS NULL
		THEN 0
	ELSE i.[Unit Volume] * sasl.Quantity
	END AS ShipmentVolume
 --,i.[Unit Volume] -- just for checking whether the calculations match
 ,(SELECT
   (DATEDIFF(dd, ssh.[Picking List Printed at], ssh.[Picking Done at]))
  -(DATEDIFF(wk, ssh.[Picking List Printed at], ssh.[Picking Done at]) * 2)
  -(CASE WHEN DATENAME(dw, ssh.[Picking List Printed at]) = 'Sunday' THEN 1 ELSE 0 END)
  -(CASE WHEN DATENAME(dw, ssh.[Picking Done at]) = 'Saturday' THEN 1 ELSE 0 END))
  AS ShipmentPickingDays

 ,(SELECT
   (DATEDIFF(dd, ssh.[Order Date], ssh.[Posting Date]))
  -(DATEDIFF(wk, ssh.[Order Date], ssh.[Posting Date]) * 2)
  -(CASE WHEN DATENAME(dw, ssh.[Order Date]) = 'Sunday' THEN 1 ELSE 0 END)
  -(CASE WHEN DATENAME(dw, ssh.[Posting Date]) = 'Saturday' THEN 1 ELSE 0 END))
  AS ShipmentDeliveryDays

FROM nav.[Sales Shipment Line] AS sasl
 LEFT JOIN nav.Customer AS cust
   ON cust.[No.] = sasl.[Sell-to Customer No.] AND cust.DWNavisionCompany = sasl.DWNavisionCompany
 LEFT JOIN nav.[Sales Shipment Header] AS ssh
   ON ssh.[No.] = sasl.[Document No.] AND ssh.DWNavisionCompany = sasl.DWNavisionCompany 
 LEFT JOIN nav.Item AS i
   ON sasl.[No.] = i.[No.] and sasl.[DWNavisionCompany] = i.DWNavisionCompany
 LEFT JOIN 
 (
   SELECT [Document No.],[Sales Shipment Line].DWNavisionCompany,Count(*) AS NoOfSL FROM [EETDW_Extract].[nav].[Sales Shipment Line] 
             INNER JOIN nav.Item ON [Sales Shipment Line].[No.] = Item.[No.] and [Sales Shipment Line].[DWNavisionCompany] = Item.DWNavisionCompany
   WHERE [Type] = '2' and Quantity &lt;&gt; 0 and [Non-physical item]=0
   GROUP BY [Document No.],[Sales Shipment Line].DWNavisionCompany
 ) countlines ON countlines.[Document No.] = sasl.[Document No.] AND countlines.DWNavisionCompany = sasl.DWNavisionCompany 

-- LEFT JOIN nav.[Cost changed Reasoncodes] ccr 
--   ON ccr.DWNavisionCompany = sasl.DWNavisionCompany AND ccr.Code = sasl.[Cost changed Reasoncode]
--WHERE sasl.[Document Type] = 1
WHERE sasl.[Type] = '2' and sasl.Quantity &lt;&gt; 0
and sasl.[Shipment Date] &gt;= CONVERT(datetime, '2012-01-01 00:00:00', 102)
--and sasl.[Document No.] = '13610527'
--and [Non-physical item]=1
--AND ssh.[Handling Fee (LCY)]&lt;&gt;0
--AND ssh.[Actual freight cost (LCY)]&lt;&gt;0</property>
