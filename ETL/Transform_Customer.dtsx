              SQLTask:BypassPrepare="False"
              SQLTask:SqlStatementSource="TRUNCATE TABLE [eetdw_extract].dbo.tmp_CustomerPreBurner;&#xA;&#xA;INSERT INTO &#xA;&#x9;[eetdw_extract].dbo.tmp_CustomerPreBurner&#xA;SELECT &#xA;&#x9;[CustomerKey]&#xA;FROM (&#xA;&#x9;SELECT DISTINCT&#xA;&#x9;&#x9;CONVERT(NVARCHAR(20),ile.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), ile.[Source No.]) AS [CustomerKey]&#xA;&#x9;FROM&#xA;&#x9;&#x9;nav.[Item Ledger Entry] AS ile&#xA;&#x9;&#x9;LEFT JOIN nav.Customer AS cust&#xA;&#x9;&#x9;ON cust.[No.] = ile.[Source No.]&#xA;&#x9;&#x9;AND cust.DWNavisionCompany = ile.DWNavisionCompany&#xA;&#x9;&#x9;LEFT JOIN nav.[Sales Invoice Header] AS sih&#xA;&#x9;&#x9;ON sih.[No.] = ile.[Document No.]&#xA;&#x9;&#x9;AND sih.DWNavisionCompany = ile.DWNavisionCompany &#xA;&#x9;&#x9;LEFT JOIN nav.[Cost changed Reasoncodes] ccr &#xA;&#x9;&#x9;ON ccr.DWNavisionCompany = ile.DWNavisionCompany&#xA;&#x9;&#x9;AND ccr.Code = ile.[Cost changed Reasoncode]&#xA;&#x9;&#x9;LEFT JOIN nav.Item AS it&#xA;&#x9;&#x9;ON it.[No.] = ile.[Item No.] AND it.DWNavisionCompany = ile.DWNavisionCompany&#xA;&#x9;WHERE&#xA;&#x9;&#x9;ile.[Entry Type] = 1&#xA;&#x9;&#x9;and ile.[Invoiced Quantity] &lt;&gt; 0&#xA;&#xA;&#x9;UNION&#xA;&#xA;&#x9;SELECT DISTINCT&#xA;&#x9;&#x9;CASE&#xA;&#x9;&#x9;&#x9;WHEN LEN(cust.[Bill-to Customer No.]) &gt; 0 &#xA;&#x9;&#x9;&#x9;THEN CONVERT(NVARCHAR(20),ile.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), cust.[Bill-to Customer No.])&#xA;&#x9;&#x9;&#x9;ELSE CONVERT(NVARCHAR(20),ile.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), ile.[Source No.]) &#xA;&#x9;&#x9;END AS [CustomerKey]&#xA;&#x9;FROM&#xA;&#x9;&#x9;nav.[Item Ledger Entry] AS ile&#xA;&#x9;&#x9;LEFT JOIN nav.Customer AS cust&#xA;&#x9;&#x9;ON cust.[No.] = ile.[Source No.] &#xA;&#x9;&#x9;AND cust.DWNavisionCompany = ile.DWNavisionCompany&#xA;&#x9;&#x9;LEFT JOIN nav.[Sales Invoice Header] AS sih&#xA;&#x9;&#x9;ON sih.[No.] = ile.[Document No.] &#xA;&#x9;&#x9;AND sih.DWNavisionCompany = ile.DWNavisionCompany &#xA;&#x9;&#x9;LEFT JOIN nav.[Cost changed Reasoncodes] ccr &#xA;&#x9;&#x9;on ccr.DWNavisionCompany = ile.DWNavisionCompany &#xA;&#x9;&#x9;AND ccr.Code = ile.[Cost changed Reasoncode]&#xA;&#x9;&#x9;LEFT JOIN nav.Item AS it&#xA;&#x9;&#x9;ON it.[No.] = ile.[Item No.] &#xA;&#x9;&#x9;AND it.DWNavisionCompany = ile.DWNavisionCompany&#xA;&#x9;WHERE&#xA;&#x9;&#x9;ile.[Entry Type] = 1&#xA;&#x9;&#x9;and ile.[Invoiced Quantity] &lt;&gt; 0&#xA;&#x9;&#xA;&#x9;UNION&#xA;&#x9;&#xA;&#x9;SELECT DISTINCT&#xA;&#x9;&#x9;CONVERT(NVARCHAR(20),rle.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), rle.[Source No.]) AS [CustomerKey]&#xA;&#x9;FROM&#xA;&#x9;&#x9;nav.[Res_ Ledger Entry] rle&#xA;&#x9;&#x9;LEFT JOIN nav.Customer AS cust&#xA;&#x9;&#x9;ON cust.[No.] = rle.[Source No.] &#xA;&#x9;&#x9;AND cust.DWNavisionCompany = rle.DWNavisionCompany&#xA;&#x9;&#x9;LEFT JOIN nav.[Sales Invoice Header] AS sih&#xA;&#x9;&#x9;ON sih.[No.] = rle.[Document No.] &#xA;&#x9;&#x9;AND sih.DWNavisionCompany = rle.DWNavisionCompany &#xA;&#x9;&#x9;LEFT JOIN nav.Resource AS rs&#xA;&#x9;&#x9;ON rs.[No.] = rle.[Resource No.] &#xA;&#x9;&#x9;AND rs.DWNavisionCompany = rle.DWNavisionCompany&#xA;&#x9;where&#xA;&#x9;&#x9;rle.[Entry Type] = 1&#xA;&#xA;&#x9;UNION&#xA;&#xA;&#x9;SELECT DISTINCT&#xA;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;WHEN LEN(cust.[Bill-to Customer No.]) &gt; 0 &#xA;&#x9;&#x9;&#x9;THEN CONVERT(NVARCHAR(20),rle.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), cust.[Bill-to Customer No.])&#xA;&#x9;&#x9;&#x9;ELSE CONVERT(NVARCHAR(20),rle.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), rle.[Source No.])&#xA;&#x9;&#x9;END AS [CustomerKey]&#xA;&#x9;FROM&#xA;&#x9;&#x9;nav.[Res_ Ledger Entry] rle&#xA;&#x9;&#x9;LEFT JOIN nav.Customer AS cust&#xA;&#x9;&#x9;ON cust.[No.] = rle.[Source No.] &#xA;&#x9;&#x9;AND cust.DWNavisionCompany = rle.DWNavisionCompany&#xA;&#x9;&#x9;LEFT JOIN nav.[Sales Invoice Header] AS sih&#xA;&#x9;&#x9;ON sih.[No.] = rle.[Document No.] &#xA;&#x9;&#x9;AND sih.DWNavisionCompany = rle.DWNavisionCompany &#xA;&#