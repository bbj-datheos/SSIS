ocument Type] = 1&#xA;&#x9;&#x9;and sl.[Type] &lt;&gt; ''&#xA;&#xA;&#x9;UNION&#xA;&#xA;&#x9;SELECT DISTINCT&#xA;&#x9;&#x9;CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sasl.[Sell-to Customer No.]) AS [CustomerKey]&#xA;&#x9;FROM&#xA;&#x9;&#x9;nav.[Sales Shipment Line] AS sasl&#xA;&#x9;&#x9;LEFT JOIN nav.Customer AS cust&#xA;&#x9;&#x9;ON cust.[No.] = sasl.[Sell-to Customer No.] &#xA;&#x9;&#x9;AND cust.DWNavisionCompany = sasl.DWNavisionCompany&#xA;&#x9;&#x9;LEFT JOIN nav.[Sales Shipment Header] AS ssh&#xA;&#x9;&#x9;ON ssh.[No.] = sasl.[Document No.] &#xA;&#x9;&#x9;AND ssh.DWNavisionCompany = sasl.DWNavisionCompany &#xA;&#x9;&#x9;LEFT JOIN nav.Item AS i&#xA;&#x9;&#x9;ON sasl.[No.] = i.[No.] &#xA;&#x9;&#x9;and sasl.[DWNavisionCompany] = i.DWNavisionCompany&#xA;&#x9;WHERE&#xA;&#x9;&#x9;sasl.[Type] = '2' &#xA;&#x9;&#x9;and sasl.Quantity &lt;&gt; 0&#xA;&#x9;&#x9;and sasl.[Shipment Date] &gt;= CONVERT(datetime, '2012-01-01 00:00:00', 102)&#xA;&#xA;UNION&#xA;&#xA;&#x9;SELECT DISTINCT &#xA;&#x9;&#x9;CASE &#xA;&#x9;&#x9;&#x9;WHEN LEN(cust.[Bill-to Customer No.]) &gt; 0 &#xA;&#x9;&#x9;&#x9;THEN CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), cust.[Bill-to Customer No.])&#xA;&#x9;&#x9;&#x9;ELSE CONVERT(NVARCHAR(20),sasl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sasl.[Sell-to Customer No.]) &#xA;&#x9;&#x9;END  AS [CustomerKey]&#xA;&#x9;FROM &#xA;&#x9;&#x9;nav.[Sales Shipment Line] AS sasl&#xA;&#x9;&#x9;LEFT JOIN nav.Customer AS cust&#xA;&#x9;&#x9;ON cust.[No.] = sasl.[Sell-to Customer No.]&#xA;&#x9;&#x9;AND cust.DWNavisionCompany = sasl.DWNavisionCompany&#xA;&#x9;&#x9;LEFT JOIN nav.[Sales Shipment Header] AS ssh&#xA;&#x9;&#x9;ON ssh.[No.] = sasl.[Document No.] &#xA;&#x9;&#x9;AND ssh.DWNavisionCompany = sasl.DWNavisionCompany &#xA;&#x9;&#x9;LEFT JOIN nav.Item AS i&#xA;&#x9;&#x9;ON sasl.[No.] = i.[No.] &#xA;&#x9;&#x9;and sasl.[DWNavisionCompany] = i.DWNavisionCompany&#xA;&#x9;WHERE &#xA;&#x9;&#x9;sasl.[Type] = 