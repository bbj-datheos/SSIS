                <component
                  refId="Package\SEQC Transform Customer\DFT Transform Customer\OLE DB Source"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="OLE DB Source"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	 CONVERT(NVARCHAR(20),c.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30),REPLACE(REPLACE([No.],CHAR(10),'NL'),CHAR(13),'CR')) as [CustomerKey]
	,CONVERT(NVARCHAR(20),c.[DWNavisionCompany]) AS CompanyCode
	,CONVERT(NVARCHAR(30),[Bill-to Customer No.]) AS CustomerBillToCustomerCode
	,CONVERT(NVARCHAR(30),c.[Name]) AS CustomerName
	,CONVERT(NVARCHAR(30),REPLACE(REPLACE([Address],CHAR(10),''),CHAR(13),''))  AS CustomerAddress
	,CONVERT(NVARCHAR(30),[Post Code]) AS CustomerPostCode
	,CONVERT(NVARCHAR(30),REPLACE(REPLACE([City],CHAR(10),''),CHAR(13),''))  AS CustomerCity
	,CONVERT(NVARCHAR(10),
		CASE 
			WHEN ([Country Code] = '' AND co.[Company Country Code] NOT IN ('G1','INT')) THEN co.[Company Country Code]
			WHEN ([Country Code] = '' AND co.[Company Country Code] IN ('G1','INT')) THEN 'DK'
			ELSE [Country Code]  
		END
	) AS CustomerCountryCode -- added by bbj 20170716
	,CONVERT(NVARCHAR(50),(
		SELECT TOP 1
			[Name]
		FROM
			[EETDW_Extract].[nav].[Country]
		WHERE
			[DWNavisionCompany]='EET Group' AND [Code]=
			CASE 
				WHEN ([Country Code] = '' AND co.[Company Country Code] NOT IN ('G1','INT')) THEN co.[Company Country Code]
				WHEN ([Country Code] = '' AND co.[Company Country Code] IN ('G1','INT')) THEN 'DK'
				ELSE [Country Code]  
			END)
	) AS CustomerCountryName -- end bbj 
	,CONVERT(NVARCHAR(20), [VAT Registration No.]) AS CustomerVatNo
	,CONVERT(NVARCHAR(20), [Company Reg. No.]) AS CustomerCompanyRegNo
	,CONVERT(NVARCHAR(20), [Fiscal Code]) AS CustomerFiscalCode
	,CONVERT(NVARCHAR(10), [Chain Name]) AS CustomerChainName
	,CONVERT(INT,Blocking) AS CustomerBlockingIndicator
	,CONVERT(NVARCHAR(10),[Gen. Bus. Posting Group]) AS CustomerGeneralBusinessPostingGroupCode
	,CONVERT(NVARCHAR(10),[Customer Posting Group]) AS CustomerPostingGroupCode
	,CONVERT(NVARCHAR(10),[Business Group No.]) AS CustomerBusinessGroupCode
	,CONVERT(NVARCHAR(10),[Sales Group No.]) AS CustomerSalesGroupCode
	,CONVERT(INT,[Insurance]) AS CustomerInsuredIndicator
	,CONVERT(NVARCHAR(10),[Insurance Company Code]) AS CustomerInsuranceCompanyCode
	,CONVERT(NVARCHAR(30),[Insurance Policy No.]) AS CustomerInsurancePolicyNo
	,CAST([Insurance Start Date] as date) as CustomerInsuranceStartDate
	,CAST([Insurance End Date] as date) as CustomerInsuranceEndDate
	,CONVERT(NVARCHAR(10),
		CASE 
			WHEN ([Currency Code] = '' OR [Currency Code] IS NULL) 
				THEN ISNULL((SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=c.[DWNavisionCompany] AND [Department Code]=c.[Department Code])
						   ,(SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=c.[DWNavisionCompany] AND [Department Code]='')) 
			ELSE [Currency Code] 
		END
	) AS CustomerCurrencyCode
	,CONVERT(NVARCHAR(10),[Price Group Code]) AS CustomerPriceGroupCode
	,CONVERT(NVARCHAR(30),[Price Profile Code]) AS CustomerPriceProfileCode
	,CONVERT(NVARCHAR(10), [Service Charge Group]) AS CustomerServiceChargeGroupCode 
	,CONVERT(NVARCHAR(30),[Item Selection Code]) AS CustomerItemSelectionCode
	,CONVERT(NVARCHAR(30),[Payment Terms Code]) AS CustomerPaymentTermsCode
	,CONVERT(INT,[Credit Control Balance Due]) AS CustomerCreditControlBalanceIndicator
	,CONVERT(NVARCHAR(10),[Salesperson Code]) AS CustomerSalespersonCode
	,CONVERT(NVARCHAR(10),'') AS CustomerEndUserSalespersonCode
	,CONVERT(NVARCHAR(30), [Shipment Method Code]) AS CustomerShipmentMethodCode
	,CONVERT(NVARCHAR(30), [Delivery Terms Code]) AS CustomerDeliveryTermsCode
	,CONVERT(INT,[Part Delivery]) AS CustomerPartDeliveryIndicator 
	,CONVERT(NVARCHAR(30),[Area Group No.]) AS CustomerAreaGroupNo
	,CONVERT(INT,[Customer Type]) AS CustomerTypeIndicator
	,CONVERT(NVARCHAR(10), [Global Business Partner]) AS CustomerGlobalBusinessPartnerCode
	,CONVERT(NVARCHAR(30), [Group-Agreement Cust. No.]) AS CustomerGroupAgreementCustomerNo
	,CONVERT(NVARCHAR(10),[Mapping Code]) AS CustomerMappingCode
	,CONVERT(NVARCHAR(10),CASE WHEN [Top 200]=1 THEN N'Yes' Else N'No' END) AS CustomerTop200 -- added by bbj 20170716
	,(SELECT [Avg Credit Days] FROM [EETDW_Extract].[nav].[Payment Terms] WHERE [Code]=c.[Payment Terms Code] AND [DWNavisionCompany]=c.[DWNavisionCompany]) AS [AvgCreditDays] -- added by bbj 20180615
	,CONVERT(NVARCHAR(50),sp.[Name]) AS CustomerSalespersonName -- added by bbj 20180615
	,CONVERT(NVARCHAR(80),sp.[Email]) AS CustomerSalespersonEmail --end bbj 
	,CONVERT(NVARCHAR(10),c.[VAT Bus. Posting Group]) AS [VATBusPostingGroup] -- added by bbj 20181121
	,CONVERT(NVARCHAR(50),ISNULL(sp.[Team Code],N'')) AS CustomerSalespersonTeam -- added by bbj 20190412
	,CONVERT(NVARCHAR(30),ISNULL(c.[County],N'')) AS [County]-- added by bbj 20190521
	,CONVERT(date,ISNULL([Created at],'1900-01-01')) as CustomerCreatedDate --- added by SRG 20191031
	,CONVERT(date,ISNULL(c.[Date of First Order],'1900-01-01')) AS CustomerFirstOrderDate ---added by SRG 20191213
	,CONVERT(date,ISNULL(c.[Date of Last Order],'1900-01-01')) AS CustomerLatestOrderDate---added by SRG 20191213
	,CONVERT(int,c.[Lifecycle Day Count]) AS CustomerLifecycleDayCount ---- SRG 20200218
	,CONVERT(nvarchar(20),
		CASE c.[Lifecycle Timespan Group]
			WHEN 0 THEN N''
			WHEN 1 THEN N'0-3M'
			WHEN 2 THEN N'4-12M'
			WHEN 3 THEN N'1-2Y'
			WHEN 4 THEN N'&gt;2Y'
		END
	) AS CustomerLifecycleTimespanGroup---- SRG 20200218
	,CONVERT(nvarchar(20),
		CASE c.[Segmentation Adjusted]
			WHEN 1 THEN N'Reseller'
			WHEN 2 THEN N'Integtator'
			WHEN 3 THEN N'Retail'
			WHEN 4 THEN N'Etailer'
			WHEN 0 THEN
				CASE c.Segmentation
					WHEN 0 THEN N''
					WHEN 1 THEN N'Reseller'
					WHEN 2 THEN N'Integtator'
					WHEN 3 THEN N'Retail'
				END
		END
	 ) AS CustomerSegmentation---- SRG 20200218
	,CONVERT(nvarchar(20),
		CASE c.Stickiness
			WHEN 0 THEN N''
			WHEN 1 THEN N'High'
			WHEN 2 THEN N'Mid'
			WHEN 3 THEN N'Low'
		END
	) AS CustomerStickiness---- SRG 20200218
FROM
	nav.Customer c
	LEFT JOIN [EETDW_Extract].[navcom].[Business Unit] bu
	ON c.DWNavisionCompany = bu.[Company Name]
	AND c.[Department Code] = bu.[Department Code]
	LEFT JOIN [EETDW_Extract].[navcom].[Company] co
	ON c.DWNavisionCompany = co.[Name]
	LEFT JOIN (
		SELECT
			[Code]
			,[DWNavisionCompany]
			,MAX([Name]) AS [Name]
			,MAX([Email]) AS [Email]
			,MAX([Team Code]) AS [Team Code]
		FROM
			[EETDW_Extract].[nav].[Salesperson/Purchaser]
		GROUP BY
			[Code],[DWNavisionCompany]
	) sp
	ON sp.Code=c.[Salesperson Code]
	AND sp.DWNavisionCompany = c.DWNavisionCompany</property>
