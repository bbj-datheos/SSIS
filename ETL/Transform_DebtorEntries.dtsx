                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	 CONVERT(nvarchar(50),[DWNavisionCompany]+ N'-'+[User ID]) AS EmployeeKey
	,Company.CompanyKey
	,CONVERT(nvarchar(10),[Document Type]) AS DocumentTypeKey
	,[Posting Date] AS CalendarKey
	,[DebtorEntryCount]
FROM (
	SELECT
		 [DWNavisionCompany]
		,[User ID]
		,[Document Type]
		,CONVERT(date,[Posting Date]) AS [Posting Date]
		,COUNT(DISTINCT [Entry No.]) AS [DebtorEntryCount]
	FROM
		[EETDW_Extract].[nav].[Cust_ Ledger Entry]
	--WHERE
	--	[Document Type] = '4'
	--	AND CONVERT(date,[Posting Date]) BETWEEN '2019-11-01' AND '2019-11-30'
	GROUP BY
		 [DWNavisionCompany]
		,[User ID]
		,[Document Type]
		,CONVERT(date,[Posting Date])
) InnerSelect
	LEFT JOIN (
		SELECT
			 Code AS CompanyKey
			,BU.[Company Name]
		FROM
			[navcom].[Business Unit] BU
		WHERE
			[Consolidate]=1
	) Company
	ON InnerSelect.DWNavisionCompany = Company.[Company Name]</property>
