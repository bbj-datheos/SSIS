                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">--[Gen. Prod. Posting Group]

SELECT DISTINCT
 CONVERT(NVARCHAR(30), bua.[UserId]) AS SystemUserKey
,CONVERT(NVARCHAR(20),ifu.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30),ifu.[Item No.]) as [ItemKey]
FROM [navcom].[BI User Access] bua
JOIN dbo.tmp_PreBurnerUserItemAccess ifu
ON ifu.[Gen. Prod. Posting Group] = bua.[Gen. Prod. Posting Group]

UNION ALL

--[Product Group]

SELECT DISTINCT
 CONVERT(NVARCHAR(30), bua.[UserId]) AS SystemUserKey
,CONVERT(NVARCHAR(20),ifu.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30),ifu.[Item No.]) as [ItemKey]
FROM [navcom].[BI User Access] bua
JOIN dbo.tmp_PreBurnerUserItemAccess ifu
ON ifu.[Product Group] = bua.[Product Group]

UNION ALL

--[Brand No.]

SELECT DISTINCT
 CONVERT(NVARCHAR(30), bua.[UserId]) AS SystemUserKey
,CONVERT(NVARCHAR(20),ifu.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30),ifu.[Item No.]) as [ItemKey]
FROM [navcom].[BI User Access] bua
JOIN dbo.tmp_PreBurnerUserItemAccess ifu
ON ifu.[Brand No.] = bua.[Brand No.]</property>
