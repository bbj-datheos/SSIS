                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/* Use temporary tables for now, create codes per the following logic:
Codes starting with 1- mean RMA status is "active" (i.e. is RMA)
Code starting with 0- means RMA status is "not active" (i.e. is not RMA)
*/
SELECT '1-' + CAST(t.[Code] AS NVARCHAR(10)) + N'-' + CAST(s.[Code] AS NVARCHAR(10)) AS [RMA key]
      ,t.[Name] AS [RMA type]
	  ,s.[Description] as [RMA status]
FROM [EETDW_Extract].[control].[EET RMA Types] as t -- use [control].[EET RMA Types] from Extract layer
CROSS JOIN  [EETDW_Extract].[control].[EET RMA Status] AS s -- use [control].[EET RMA Status] from Extract layer
UNION
SELECT '0-0-0', '', ''</property>
