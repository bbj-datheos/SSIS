                <component
                  refId="Package\SEQC Transform SalesOrder\DFT Transform SalesOrder\OLE SRC Extract"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SqlServer v10; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  name="OLE SRC Extract"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT --TOP 1000
	ISNULL(
		(
			SELECT MAX([Code])
			FROM [EETDW_Extract].[navcom].[Business Unit]
			WHERE [Consolidate]=1 AND [Company Name]=sl.[DWNavisionCompany]	AND [Department Code]=sl.[Department Code]
		)
		,(
			SELECT MAX([Code])
			FROM [EETDW_Extract].[navcom].[Business Unit]
			WHERE [Consolidate]=1 AND [Company Name]=sl.[DWNavisionCompany] AND [Department Code]=''
		)
	) AS [CompanyKey]
	,CONVERT(NVARCHAR(20),sl.[Document No.]) AS SalesNo
	,CONVERT(NVARCHAR(30), sl.[Line No.]) AS SalesLineNo
	,CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sl.[Sell-to Customer No.]) AS [CustomerSellToKey]
	,CASE 
		WHEN LEN(cust.[Bill-to Customer No.]) &gt; 0 THEN CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), cust.[Bill-to Customer No.])
		ELSE CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(30), sl.[Sell-to Customer No.]) 
	 END AS [CustomerBillToKey]
	,CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CASE WHEN sl.[Type]=3 THEN N'RES_' ELSE N'' END + CONVERT(NVARCHAR(50), sl.[No.]) AS ItemKey
	,sl.[Order Date] AS CalendarKey
	,sl.[Order Date] AS CalendarOrderKey
	,ISNULL(sl.[Shipment Date],sh.[Shipment Date]) AS CalendarShippingKey -- added SRG 20191031
	,CONVERT(NVARCHAR(20),sh.[Origin of order]) AS OriginOfOrderKey
	,CASE
		WHEN (sl.[Currency Code] = '' OR sl.[Currency Code] IS NULL)
		THEN 
		  CASE WHEN (sh.[Currency Code] = '' OR sh.[Currency Code] IS NULL)
		  THEN ISNULL((SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=sh.[DWNavisionCompany] AND [Department Code]=sh.[Department Code])
	                 ,(SELECT MAX([Local Currency Code]) FROM [EETDW_Extract].[navcom].[Business Unit] WHERE [Consolidate]=1 AND [Company Name]=sh.[DWNavisionCompany] AND [Department Code]=''))
		  ELSE sh.[Currency Code]
		END  
		  ELSE sl.[Currency Code]
	 END AS [CurrencyKey]
	,CONVERT(nvarchar(10),
		CASE 
			WHEN LEN(sl.[RMA No]) &gt; 0 THEN N'1-0-0-' + ISNULL(CONVERT(nvarchar(10),ReasonCode.[No.]),N'0') -- added by BBJ 20181214 to support new blanket order type
			WHEN sl.[Document Type]=4 THEN N'0-1-0-0' -- end bbj	
			ELSE N'0-0-0-0' 
		END
	 ) AS [OrderTypeKey]
	,CASE
		WHEN ISNULL(sl.[Bid No],N'')='' THEN CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sl.[Cost changed Reasoncode])
		ELSE CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sl.[Cost changed Reasoncode]) + N'-' + CONVERT(NVARCHAR(30),sl.[Bid No])
	END AS CostChangeReasonKey
	,CONVERT(NVARCHAR(20),sl.[DWNavisionCompany]) + N'-' + CONVERT(NVARCHAR(20),sh.[Salesperson Code]) AS EmployeeKey
	,CONVERT(INT,pn.ID) AS PersonKey
	,CONVERT(NVARCHAR(20),sh.[Order Status]) + N'-' + CONVERT(NVARCHAR(20),sh.[Hold invoice]) AS OrderStatusKey
	,CASE
		WHEN sl.Quantity&lt;&gt;0 AND (sl.Quantity-sl.[Quantity Invoiced])&lt;&gt;0 THEN (sl.Quantity-sl.[Quantity Invoiced])*sl.[Amount (LCY)]/sl.Quantity
		WHEN sl.Quantity&lt;&gt;0 AND (sl.Quantity-sl.[Quantity Invoiced])=0 AND sl.[Order Date] =  CONVERT(date, getdate()) THEN sl.[Amount (LCY)]
		ELSE 0
	END AS SalesAmount
	,CASE
		WHEN sl.Quantity&lt;&gt;0  AND (sl.Quantity-sl.[Quantity Invoiced])&lt;&gt;0 THEN (sl.Quantity-sl.[Quantity Invoiced])*sl.[Order Cost Amount (LCY)]/sl.Quantity
		WHEN sl.Quantity&lt;&gt;0 AND (sl.Quantity-sl.[Quantity Invoiced])=0 AND sl.[Order Date] =  CONVERT(date, getdate()) THEN sl.[Order Cost Amount (LCY)]
		ELSE 0
	END AS OrderCostAmount -- Sales Cost
	,CASE
		WHEN sl.Quantity&lt;&gt;0  AND (sl.Quantity-sl.[Quantity Invoiced])&lt;&gt;0 THEN (sl.Quantity-sl.[Quantity Invoiced])*sl.[Order Profit Margin (LCY)]/sl.Quantity
		WHEN sl.Quantity&lt;&gt;0 AND (sl.Quantity-sl.[Quantity Invoiced])=0 AND sl.[Order Date] =  CONVERT(date, getdate()) THEN sl.[Order Profit Margin (LCY)]
		ELSE 0
	END AS OrderProfitMarginAmount -- Sales Profit
	,CASE
		WHEN sl.Quantity&lt;&gt;0  THEN (sl.Quantity-sl.[Quantity Invoiced])*sl.[Unit Cost (LCY)]*-1/sl.Quantity
		ELSE sl.[Unit Cost (LCY)]*-1
	END AS UnitCostAmount -- Financial cost
	,CONVERT(numeric(18,6),ISNULL([Units per Parcel],0)) AS UnitsPerParcel -- added by SRG: 20191112
	,sl.[Amount (LCY)]
	,sl.Quantity
	,sl.Quantity-sl.[Quantity Invoiced] as [Outstanding Quantity] -- added by bbj 20180828
	,CONVERT(NVARCHAR(100),N'SO-' + sh.[No.] + N'-' + ISNULL(sh.[Org. Sales Order No.],N'')) AS DocumentKey --end bbj
	,CONVERT(nvarchar(50),sl.[DWNavisionCompany]+N'-'+sl.[Owner] +N'-'+ sl.[Location Code]) AS [InventoryTypeKey] 	-- added by bbj 20180913 -- changed SRG 20191031
FROM
	nav.[Sales Line Live] AS sl
	LEFT JOIN nav.Customer AS cust ON cust.[No.] = sl.[Sell-to Customer No.] AND cust.DWNavisionCompany = sl.DWNavisionCompany
	LEFT JOIN nav.[Sales Header Live] AS sh ON sh.[No.] = sl.[Document No.] AND sh.DWNavisionCompany = sl.DWNavisionCompany 
	LEFT JOIN nav.[Cost changed Reasoncodes] ccr ON ccr.DWNavisionCompany = sl.DWNavisionCompany AND ccr.Code = sl.[Cost changed Reasoncode]
	--LEFT JOIN navcom.[System User] su
	--  ON su.[Primary Company] = sh.DWNavisionCompany AND su.[User ID] = sh.[Salesperson Code]
	LEFT JOIN EETDW_Stage.map.PersonNavision pn on pn.SystemUser = sh.[Salesperson Code]
	LEFT JOIN (
		SELECT DISTINCT
			CONVERT(nvarchar(10),[No.]) AS [No.]
		FROM
			[nav].[RMA Reasoncode]
		WHERE
			[DWNavisionCompany] = 'EET Group'
	) AS ReasonCode
	ON ReasonCode.[No.] = sh.[Reason Code]
	--Added to only show the english reason code, as the dimension is kind of handheld.
	--If it becomes more dynamic, the where should be removed.
	--ADDED by SRG 20191008
WHERE
	sl.[Document Type] IN (1,4)  --- sl.[Document Type] = 1  changed by bbj 20180928 due to new blanket order
	and sl.[Type] &lt;&gt; ''
	and sl.[No.]&lt;&gt;''
	and not (sl.[Document Type] =1 AND sl.[Blanket Order No.]&lt;&gt;'')    ---changed by bbj 20180928 due to new blanket order
	-- added by bbj 20180327
	--and sl.Quantity&lt;&gt;0 not needed since included in cas on values
	--and sl.Quantity=0
	-- end bbj
	--and (sl.[Outstanding Quantity]&lt;&gt;0 OR sl.[Qty. Shipped Not Invoiced]&lt;&gt;0)
	and ((sl.Quantity-sl.[Quantity Invoiced])&lt;&gt;0 OR ( (sl.Quantity-sl.[Quantity Invoiced])=0 AND sl.[Order Date] =  CONVERT(date, getdate())  ) ) --'2019-05-24'
	--and (sl.Quantity-sl.[Quantity Invoiced])=0
	--((sl.[Outstanding Quantity]&lt;&gt;0 AND sh.[Hold invoice]=0) OR (sh.[Hold invoice]=1 AND sl.[Outstanding Quantity]=0)) -- 20180307 added by bbj after agreement with Jan Iversen
	--and sh.[Hold invoice]=1
	--and sl.DWNavisionCompany='EET Finland'
	--and sl.[Qty. Shipped Not Invoiced]&lt;&gt;0
	--and sl.[Document No.]='1731307'</property>
