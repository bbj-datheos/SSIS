                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/****** Script for SelectTopNRows command from SSMS  ******/


WITH ExchangeRate AS
(

SELECT 
       bu.Code AS CompanyKey
--	   ,bu.[Business Unit Name]
      ,[Currency Code]
	  ,bu.[Reporting Currency Code]
	  ,100*YEAR([Period])+MONTH([Period]) AS MonthID
	  ,[Local Currency Code] AS CurrencyKey
      ,[Exchange Rate Type] + 1 AS [ExchangeRateTypeID]
      ,[Rel. Exchange Rate Amount]
 --     ,[Exchange Rate Type]

  FROM 
   
(

SELECT 
      [Currency Code]
      ,[Period]
      ,-1 AS [Exchange Rate Type]
      ,100 AS [Rel. Exchange Rate Amount]
FROM [EETDW_Extract].[navcom].[Reporting Exchange Rate]
GROUP BY [Currency Code],[Period]
--ORDER BY 1,2,3
UNION ALL
SELECT 
      [Currency Code]
      ,[Period]
      ,[Exchange Rate Type]
	  ,[Rel. Exchange Rate Amount]
FROM [EETDW_Extract].[navcom].[Reporting Exchange Rate] WHERE [Exchange Rate Type]&lt;&gt;3
--ORDER BY 1,2,3
UNION ALL
SELECT 
      [Currency Code]
      ,[Period]
      ,3 AS [Exchange Rate Type]
      ,[Rel. Exchange Rate Amount]
FROM [EETDW_Extract].[navcom].[Reporting Exchange Rate] WHERE [Exchange Rate Type]=2
--ORDER BY 1,2,3


  ) rer
  INNER JOIN [EETDW_Extract].[navcom].[Business Unit] bu 
       ON (bu.Consolidate=1 AND [Currency Code]=bu.[Reporting Currency Code])
	       OR (bu.Consolidate=1 AND [Currency Code]=LEFT(bu.[Reporting Currency Code],3) AND rer.[Exchange Rate Type] NOT IN (-1,2,3)) -- NB Exclude duplets

WHERE [Period]&gt;='2014-01-01'
AND bu.Consolidate=1

) 
--AND bu.Code='60'  -- 60 UK  56 DE
--AND 100*YEAR([Period])+MONTH([Period])=201601
--ORDER BY CAST(bu.Code AS INT),2,3,4

--order by 4,1,6




SELECT 
	CompanyKey
	,[Currency Code]
	,[Reporting Currency Code]
	,MonthID
	,CurrencyKey
	,[ExchangeRateTypeID]
	,[Rel. Exchange Rate Amount]
FROM ExchangeRate

UNION ALL

SELECT 
	CompanyKey
	,[Currency Code]
	,[Reporting Currency Code]
	,201312 as MonthID
	,CurrencyKey
	,[ExchangeRateTypeID]
	,[Rel. Exchange Rate Amount]
FROM ExchangeRate
WHERE MonthID = 201401</property>
